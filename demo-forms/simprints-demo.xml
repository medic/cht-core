<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Simprints Registration Demo</h:title>
    <model>
      <instance>
        <data delimiter="#" id="simprints_registration_demo">
          <name/>
          <simprints_id/>
          <meta>
            <instanceID/>
          </meta>
        </data>
      </instance>
      <bind nodeset="/data/name" type="string"/>
      <bind nodeset="/data/simprints_id" type="string"/>
    </model>
  </h:head>
  <h:body class="pages">
    <group appearance="field-list" ref="/data">
      <label>Register someone with simprints</label>
      <input ref="/data/name">
        <label>Name</label>
      </input>
      <input ref="/data/simprints_id" appearance="simprints-reg">
        <label>Simprints ID</label>
      </input>
    </group>
  </h:body>
</h:html>
