{
	// Run the test, but don't send any emails. Optional.
	"dryrun": true,

	// Dir from which to read the logs. Expecting log files names that contain "sentinel".
	// Absolute path!
	"logdir": "/srv/storage/gardener/logs/", // sentinel logs are in gardener dir
	// String to look for in logs, which indicate the monitored problem happened.
	"errorString": " - info: info: startup complete.",  // sentinel restart

	// Will alert if more than maxNumRestarts within the last ageLimitMinutes.
	"maxNumRestarts": 5,
	"ageLimitMinutes": 60,

	// Email alerts
	"sender" : {
		"email": "sentinel.monitor.medicmobile@gmail.com",
//		"password": "<password>"
		},
	"recipients" : [
		// "person@something.com", "other.person@somethingelse.org"
	]
}