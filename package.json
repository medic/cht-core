{
  "name": "medic",
  "version": "4.3.0",
  "private": true,
  "license": "AGPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "git://github.com/medic/cht-core.git"
  },
  "engines": {
    "node": ">=16.12.0",
    "npm": ">=8.3.1"
  },
  "workspaces": [
    "./shared-libs/*"
  ],
  "scripts": {
    "test": "npm run lint && grunt unit && grunt test-api-integration",
    "lint": "eslint --color --cache . && ./scripts/build/blank-link-check.sh",
    "lint-service-worker": "eslint --color --cache --no-ignore -c ./.eslintrc build/service-worker.js",
    "wdio-local": "export VERSION=$(node ./scripts/build/get-version.js) && grunt e2e-env-setup && wdio run ./tests/e2e/default/wdio.conf.js --suite=all",
    "standard-wdio-local": "export VERSION=$(node ./scripts/build/get-version.js) && grunt e2e-env-setup && wdio run ./tests/e2e/standard/wdio.conf.js --suite=all",
    "default-wdio-mobile-local": "export VERSION=$(node ./scripts/build/get-version.js) && grunt e2e-env-setup && wdio run ./tests/e2e/default-mobile/wdio.conf.js --suite=all",
    "ci-e2e-integration": "mocha --config tests/integration/.mocharc.js && npm run lint-service-worker",
    "e2e-integration-local": "export VERSION=$(node ./scripts/build/get-version.js) && grunt e2e-env-setup && npm run ci-e2e-integration",
    "ci-webdriver-default": "wdio run ./tests/e2e/default/wdio.conf.js",
    "upgrade-wdio": "wdio run ./tests/e2e/upgrade/wdio.conf.js",
    "ci-webdriver-standard": "wdio run ./tests/e2e/standard/wdio.conf.js",
    "ci-webdriver-default-mobile": "wdio run ./tests/e2e/default-mobile/wdio.conf.js",
    "local-images": "export VERSION=$(node ./scripts/build/get-version.js) && grunt build-service-images && npm run create-local-docker-compose-files",
    "prepare": "husky install",
    "postinstall": "cd webapp && ng cache clean && cd ..",
    "mocha-unit-webapp": "UNIT_TEST_ENV=1 mocha 'webapp/tests/mocha/**/*.spec.js'",
    "mocha-unit-api": "UNIT_TEST_ENV=1 mocha 'api/tests/mocha/**/*.js'",
    "mocha-unit-sentinel": "UNIT_TEST_ENV=1 mocha 'sentinel/tests/**/*.js'",
    "mocha-integration-api": "mocha 'api/tests/integration/**/*.js' -t 10000",
    "jsdoc-admin": "jsdoc -d jsdocs/admin -c node_modules/angular-jsdoc/common/conf.json -t node_modules/angular-jsdoc/angular-template admin/src/js/**/*.js",
    "jsdoc-sentinel": "jsdoc -d jsdocs/sentinel sentinel/src/**/*.js",
    "jsdoc-api": "jsdoc -d jsdocs/api -R api/README.md api/src/**/*.js",
    "jsdoc-shared-libs": "jsdoc -d jsdocs/shared-libs shared-libs/**/src/**/*.js",
    "build-documentation": "npm run jsdoc-admin && npm run jsdoc-sentinel && npm run jsdoc-api && npm run jsdoc-shared-libs",
    "dev-api": "npm run copy-api-bowser && TZ=UTC nodemon --inspect=0.0.0.0:9229 --ignore 'api/build/static' --ignore 'api/build/public' --watch api --watch 'shared-libs/**/src/**' api/server.js -- --allow-cors",
    "dev-sentinel": "TZ=UTC nodemon --inspect=0.0.0.0:9228 --watch sentinel --watch 'shared-libs/**/src/**' sentinel/server.js",
    "css-enketo": "sass webapp/src/css/enketo/enketo.scss api/build/static/webapp/enketo.less --no-source-map",
    "css-admin": "lessc admin/src/css/main.less api/build/static/admin/css/main.css",
    "browserify-admin": "mkdir -p api/build/static/admin/js/ && browserify --debug -t browserify-ngannotate -r './admin/node_modules/angular-translate/dist/angular-translate-interpolation-messageformat/angular-translate-interpolation-messageformat:angular-translate-interpolation-messageformat' -r './admin/node_modules/google-libphonenumber:google-libphonenumber' -r './admin/node_modules/gsm:gsm' -r './admin/node_modules/object-path:object-path' -r './admin/node_modules/bikram-sambat:bikram-sambat' -r './admin/node_modules/lodash/core:lodash/core' admin/src/js/main.js > api/build/static/admin/js/main.js",
    "uglify-api": "uglifyjs api/build/static/login/script.js -o api/build/static/login/script.js && uglifyjs api/build/static/login/lib-bowser.js -o api/build/static/login/lib-bowser.js",
    "uglify-admin": "uglifyjs api/build/static/admin/js/main.js -o api/build/static/admin/js/main.js && uglifyjs api/build/static/admin/js/templates.js -o api/build/static/admin/js/templates.js",
    "test-config-standard": "cd config/standard && npm ci && npm run ci",
    "test-config-default": "cd config/default && npm ci && npm run test",
    "shared-lib-unit": "UNIT_TEST_ENV=1 npm test --workspaces --if-present",
    "cleancss-admin": "cleancss api/build/static/admin/css/main.css > out.css && mv out.css api/build/static/admin/css/main.css",
    "cleancss-api": "cleancss api/build/static/login/style.css > out.css && mv out.css api/build/static/login/style.css",
    "build-config": "node ./node_modules/medic-conf/src/bin/medic-conf.js --skip-dependency-check --archive --source=config/default --destination=api/build/default-docs upload-app-settings upload-app-forms upload-collect-forms upload-contact-forms upload-resources upload-custom-translations",
    "copy-api-bowser": "cp api/node_modules/bowser/bundled.js api/src/public/login/lib-bowser.js",
    "create-staging-doc": "node scripts/build/cli createStagingDoc",
    "populate-staging-doc": "node scripts/build/cli populateStagingDoc",
    "create-local-docker-compose-files": "node scripts/build/cli localDockerComposeFiles",
    "update-service-worker": "node scripts/build/cli updateServiceWorker",
    "set-ddocs-version": "node scripts/build/cli setDdocsVersion",
    "set-build-info": "node scripts/build/cli setBuildInfo",
    "compile-ddocs-primary": "node ./scripts/build/ddoc-compile.js primary",
    "compile-ddocs-staging": "node ./scripts/build/ddoc-compile.js staging",
    "push-ddoc-to-staging": "node ./scripts/build/push-ddoc-to-staging.js",
    "clean-build-dir": "rm -rf build && mkdir build",
    "compile-admin-templates": "mkdir -p api/build/static/admin/js/ && node ./scripts/build/build-angularjs-template-cache.js",
    "karma-admin": "node ./scripts/ci/run-karma.js",
    "copy-ddocs": "mkdir -p build/ddocs && cp -r ddocs/* build/ddocs/",
    "copy-api-ddocs": "mkdir -p api/build/ddocs && cp build/ddocs/*.json api/build/ddocs/",
    "copy-webapp-static": "cp -r webapp/src/audio webapp/src/fonts webapp/src/img api/build/static/webapp/",
    "copy-api-resources": "cp -r api/src/public/* api/build/static/",
    "copy-admin-static": "cp -r admin/src/templates/index.html admin/node_modules/font-awesome/fonts webapp/src/fonts api/build/static/admin",
    "npm-ci-modules": "node scripts/build/cli npmCiModules",
    "check-version": "node scripts/ci/check-versions.js",
    "build-service-images": "node scripts/build/cli buildServiceImages",
    "build-images": "node scripts/build/cli buildImages",
    "publish-service-images": "mkdir -p images && node scripts/build/cli publishServiceImages"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^15.0.0",
    "@angular-devkit/build-angular": "^15.0.5",
    "@angular-eslint/builder": "^15.1.0",
    "@angular-eslint/eslint-plugin": "^15.1.0",
    "@angular-eslint/eslint-plugin-template": "^15.1.0",
    "@angular-eslint/schematics": "^15.1.0",
    "@angular-eslint/template-parser": "^15.1.0",
    "@angular/cli": "^15.0.5",
    "@angular/compiler": "^15.0.4",
    "@angular/compiler-cli": "^15.0.4",
    "@angular/localize": "^15.0.4",
    "@faker-js/faker": "^6.1.2",
    "@medic/eslint-config": "^1.1.0",
    "@types/chai": "^4.3.4",
    "@types/chai-as-promised": "^7.1.5",
    "@types/jquery": "^3.5.6",
    "@types/lodash": "^4.14.175",
    "@types/lodash-es": "^4.17.6",
    "@types/mocha": "^10.0.1",
    "@types/node": "^14.18.34",
    "@types/pouchdb": "^6.4.0",
    "@types/sinon": "^10.0.11",
    "@typescript-eslint/eslint-plugin": "5.43",
    "@typescript-eslint/parser": "5.43",
    "@wdio/allure-reporter": "^8.3.0",
    "@wdio/cli": "^8.3.3",
    "@wdio/devtools-service": "^8.3.3",
    "@wdio/local-runner": "^8.3.3",
    "@wdio/mocha-framework": "^8.3.0",
    "@wdio/spec-reporter": "^8.3.0",
    "allure-commandline": "^2.20.1",
    "angular-jsdoc": "^1.5.1",
    "angular-mocks": "^1.8.3",
    "bikram-sambat": "^1.7.0",
    "browserify": "^17.0.0",
    "browserify-ngannotate": "^2.0.0",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "chai-exclude": "^2.1.0",
    "chai-shallow-deep-equal": "^1.4.6",
    "cht-conf": "^3.15.2",
    "clean-css-cli": "^5.6.2",
    "couchdb-compile": "^1.11.2",
    "csv-writer": "^1.6.0",
    "deep-equal-in-any-order": "^1.1.15",
    "doctoc": "^2.2.1",
    "eslint": "^7.32.0",
    "eslint-plugin-angular": "^4.1.0",
    "eslint-plugin-async": "^0.1.1",
    "eslint-plugin-compat": "^3.13.0",
    "eslint-plugin-couchdb": "^0.2.0",
    "eslint-plugin-jasmine": "^4.1.3",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "eurodigit": "^3.1.3",
    "express": "^4.18.2",
    "faker": "^5.5.3",
    "flat": "^5.0.2",
    "gaze": "^1.1.3",
    "grunt": "^1.5.3",
    "grunt-exec": "^3.0.0",
    "husky": "^8.0.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jsdoc": "^4.0.2",
    "jsverify": "^0.8.4",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage": "^2.2.0",
    "karma-firefox-launcher": "^2.1.2",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-ng-html2js-preprocessor": "^1.0.0",
    "karma-spec-reporter": "0.0.32",
    "less": "^4.1.3",
    "lodash": "4.17.19",
    "loose-envify": "^1.4.0",
    "medic-conf": "^3.2.1",
    "mocha": "^10.2.0",
    "module-alias": "^2.2.2",
    "moment": "^2.29.4",
    "moment-locales-webpack-plugin": "^1.2.0",
    "mustache": "^4.2.0",
    "nodemon": "^2.0.20",
    "patch-package": "^7.0.2",
    "postcss": "^8.4.20",
    "pouchdb-adapter-http": "^7.3.1",
    "pouchdb-adapter-memory": "^7.3.1",
    "pouchdb-core": "^7.3.1",
    "pouchdb-mapreduce": "^7.3.1",
    "raw-loader": "^4.0.2",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
    "rewire": "^5.0.0",
    "rosie": "^2.1.0",
    "sass": "^1.57.1",
    "semver": "^7.3.8",
    "sinon": "^11.1.2",
    "tail": "^2.2.4",
    "typescript": "4.8",
    "uglify-js": "^3.17.4",
    "uuid": "^8.3.2",
    "webpack-bundle-analyzer": "^4.7.0",
    "zone.js": "^0.11.8"
  },
  "overrides": {
    "minimist": ">=1.2.6",
    "nools": {
      "uglify-js": ">=2.6.0"
    }
  }
}
