    "pouchdb-session-authentication": "^1.1.0",
    "raw-loader": "^4.0.2",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
    "rewire": "^7.0.0",
    "rosie": "^2.1.0",
    "sass": "^1.67.0",
    "semver": "^7.5.4",
    "shellcheck": "^2.2.0",
    "sinon": "^16.1.0",
    "tail": "^2.2.6",
    "typescript": "^5.3.3",
    "uglify-js": "^3.17.4",
    "uuid": "^9.0.1",
    "wdio-chromedriver-service": "^8.1.1",
    "webpack-bundle-analyzer": "^4.9.1",
    "zone.js": "^0.14.4"
  },
  "overrides": {
    "minimist": ">=1.2.6",
    "nools": {
      "uglify-js": ">=2.6.0"
    }
  },
  "dependencies": {
    "google-libphonenumber": "^3.2.31"
  }
}
