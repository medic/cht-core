      <bind calculate="&quot;district_hospital&quot;" nodeset="/data/init/place_type" type="select1"/>
      <bind calculate="jr:choice-name( /data/init/place_type ,' /data/init/place_type ')" nodeset="/data/init/place_type_translation" type="string"/>
      <bind calculate="&quot;district_hospital&quot;" nodeset="/data/init/generated_name" type="select1"/>
      <bind calculate="jr:choice-name( /data/init/generated_name ,' /data/init/generated_name ')" nodeset="/data/init/generated_name_translation" type="string"/>
      <bind calculate="." nodeset="/data/init/name" type="string"/>
      <bind calculate="../name" nodeset="/data/init/contact_name" type="string"/>
      <bind calculate="../../district_hospital/contact" nodeset="/data/init/select_person" type="db:person"/>
      <bind calculate="../../district_hospital/name" nodeset="/data/init/custom_place_name" relevant="selected( /data/district_hospital/is_name_generated , 'false')" required="true()" type="string"/>
      <bind calculate="." nodeset="/data/district_hospital/parent" readonly="true()" type="string"/>
      <bind calculate="." nodeset="/data/district_hospital/type" readonly="true()" type="string"/>
      <bind nodeset="/data/district_hospital/is_name_generated" required="true()" type="select1"/>
      <bind calculate="if(  /data/district_hospital/is_name_generated  = 'true',  /data/init/generated_name_translation , coalesce( /data/init/custom_place_name ,.))" nodeset="/data/district_hospital/name" type="string"/>
      <bind nodeset="/data/district_hospital/external_id" type="string"/>
      <bind nodeset="/data/district_hospital/use_cases" relevant="../type = 'district_hospital'" type="select"/>
      <bind nodeset="/data/district_hospital/notes" type="string"/>
      <bind calculate="coalesce( /data/init/select_person , .)" nodeset="/data/district_hospital/contact" type="string"/>
      <bind calculate="concat(../../inputs/meta/location/lat, concat(' ', ../../inputs/meta/location/long))" nodeset="/data/district_hospital/geolocation" type="string"/>
      <bind calculate="." nodeset="/data/district_hospital/meta/created_by" readonly="true()" type="string"/>
      <bind calculate="." nodeset="/data/district_hospital/meta/created_by_person_uuid" readonly="true()" type="string"/>
      <bind calculate="." nodeset="/data/district_hospital/meta/created_by_place_uuid" type="string"/>
      <bind calculate="../../../inputs/user/name" nodeset="/data/district_hospital/meta/last_edited_by" type="string"/>
      <bind calculate="../../../inputs/user/contact_id" nodeset="/data/district_hospital/meta/last_edited_by_person_uuid" type="string"/>
      <bind calculate="../../../inputs/user/facility_id" nodeset="/data/district_hospital/meta/last_edited_by_place_uuid" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group ref="/data/inputs">
      <label ref="jr:itext('/data/inputs:label')"/>
      <group ref="/data/inputs/user">
        <label ref="jr:itext('/data/inputs/user:label')"/>
        <input ref="/data/inputs/user/contact_id">
          <label ref="jr:itext('/data/inputs/user/contact_id:label')"/>
        </input>
        <input ref="/data/inputs/user/facility_id">
          <label ref="jr:itext('/data/inputs/user/facility_id:label')"/>
        </input>
        <input ref="/data/inputs/user/name">
          <label ref="jr:itext('/data/inputs/user/name:label')"/>
        </input>
      </group>
    </group>
    <group appearance="field-list" ref="/data/init">
      <label ref="jr:itext('/data/init:label')"/>
      <select1 appearance="hidden" ref="/data/init/place_type">
        <label ref="jr:itext('/data/init/place_type:label')"/>
        <item>
          <label ref="jr:itext('/data/init/place_type/district_hospital:label')"/>
          <value>district_hospital</value>
        </item>
        <item>
          <label ref="jr:itext('/data/init/place_type/health_center:label')"/>
          <value>health_center</value>
        </item>
        <item>
          <label ref="jr:itext('/data/init/place_type/clinic:label')"/>
          <value>clinic</value>
        </item>
      </select1>
      <select1 appearance="hidden" ref="/data/init/generated_name">
        <label ref="jr:itext('/data/init/generated_name:label')"/>
        <item>
          <label ref="jr:itext('/data/init/generated_name/district_hospital:label')"/>
          <value>district_hospital</value>
        </item>
        <item>
          <label ref="jr:itext('/data/init/generated_name/health_center:label')"/>
          <value>health_center</value>
        </item>
        <item>
          <label ref="jr:itext('/data/init/generated_name/clinic:label')"/>
          <value>clinic</value>
        </item>
      </select1>
      <input appearance="db-object" ref="/data/init/select_person">
        <label ref="jr:itext('/data/init/select_person:label')"/>
        <hint ref="jr:itext('/data/init/select_person:hint')"/>
      </input>
    </group>
    <group appearance="field-list" ref="/data/district_hospital">
      <label ref="jr:itext('/data/district_hospital:label')"/>
      <select1 appearance="horizontal-compact" ref="/data/district_hospital/is_name_generated">
        <label ref="jr:itext('/data/district_hospital/is_name_generated:label')"/>
        <item>
          <label ref="jr:itext('/data/district_hospital/is_name_generated/true:label')"/>
          <value>true</value>
        </item>
        <item>
          <label ref="jr:itext('/data/district_hospital/is_name_generated/false:label')"/>
          <value>false</value>
        </item>
      </select1>
      <input ref="/data/init/custom_place_name">
        <label ref="jr:itext('/data/init/custom_place_name:label')"/>
        <hint ref="jr:itext('/data/init/custom_place_name:hint')"/>
      </input>
      <input ref="/data/district_hospital/external_id">
        <label ref="jr:itext('/data/district_hospital/external_id:label')"/>
      </input>
      <select ref="/data/district_hospital/use_cases">
        <label ref="jr:itext('/data/district_hospital/use_cases:label')"/>
        <hint ref="jr:itext('/data/district_hospital/use_cases:hint')"/>
        <item>
          <label ref="jr:itext('/data/district_hospital/use_cases/anc:label')"/>
          <value>anc</value>
        </item>
        <item>
          <label ref="jr:itext('/data/district_hospital/use_cases/pnc:label')"/>
          <value>pnc</value>
        </item>
        <item>
          <label ref="jr:itext('/data/district_hospital/use_cases/imm:label')"/>
          <value>imm</value>
        </item>
      </select>
      <input appearance="multiline" ref="/data/district_hospital/notes">
        <label ref="jr:itext('/data/district_hospital/notes:label')"/>
      </input>
      <group appearance="hidden" ref="/data/district_hospital/meta">
        <label ref="jr:itext('/data/district_hospital/meta:label')"/>
      </group>
    </group>
  </h:body>
</h:html>
