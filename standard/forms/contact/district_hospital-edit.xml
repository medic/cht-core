      <instance>
        <data delimiter="#" id="district_hospital_edit" prefix="J1!district_hospital_edit!" version="2017-05-04_07-08">
          <inputs>
            <meta>
              <location>
                <lat/>
                <long/>
                <error/>
                <message/>
              </location>
            </meta>
            <user>
              <contact_id/>
              <facility_id/>
              <name/>
            </user>
          </inputs>
          <district_hospital>
            <parent/>
            <type/>
            <is_name_generated/>
            <name/>
            <external_id/>
            <use_cases/>
            <notes/>
            <contact/>
            <geolocation/>
            <meta>
              <created_by/>
              <created_by_person_uuid/>
              <created_by_place_uuid/>
              <last_edited_by/>
              <last_edited_by_person_uuid/>
              <last_edited_by_place_uuid/>
            </meta>
          </district_hospital>
          <init>
            <place_type/>
            <place_type_translation/>
            <generated_name/>
            <generated_name_translation/>
            <name/>
            <contact_name/>
            <select_person/>
            <custom_place_name/>
          </init>
          <meta>
            <instanceID/>
          </meta>
        </data>
      </instance>
      <bind nodeset="/data/inputs/user" relevant="false()"/>
      <bind nodeset="/data/inputs/user/contact_id" type="string"/>
      <bind nodeset="/data/inputs/user/facility_id" type="string"/>
