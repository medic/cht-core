      <instance>
        <pnc delimiter="#" id="pnc" prefix="J1!pnc!" version="2017-08-12_06-50">
          <id/>
          <who/>
          <dm/>
          <db/>
          <n/>
          <rm/>
          <rb/>
          <v/>
          <r_referral_note/>
          <meta>
            <instanceID/>
          </meta>
        </pnc>
      </instance>
      <bind nodeset="/pnc/id" required="true()" type="string"/>
      <bind nodeset="/pnc/who" required="true()" type="select1"/>
      <bind nodeset="/pnc/dm" relevant=" /pnc/who  = 'mom' or  /pnc/who  = 'both'" type="select"/>
      <bind nodeset="/pnc/db" relevant=" /pnc/who  = 'baby' or  /pnc/who  = 'both'" type="select"/>
      <bind nodeset="/pnc/n" type="string"/>
      <bind calculate="if (count-selected( /pnc/dm ) &gt; 0, 'true', 'false')" nodeset="/pnc/rm" type="string"/>
      <bind calculate="if (count-selected( /pnc/db ) &gt; 0, 'true', 'false')" nodeset="/pnc/rb" type="string"/>
      <bind calculate="&quot;yes&quot;" nodeset="/pnc/v" type="string"/>
      <bind nodeset="/pnc/r_referral_note" readonly="true()" relevant="count-selected( /pnc/dm ) &gt; 0 or count-selected( /pnc/db ) &gt; 0" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/pnc/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
