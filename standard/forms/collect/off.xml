<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Turn Notifications OFF</h:title>
    <model>
      <itext>
        <translation lang="Bahasa Indonesia (Indonesian)">
          <text id="/off/r:label">
            <value>Notes</value>
          </text>
          <text id="/off/review:label">
            <value>Submitting report to turn OFF notifications for ID <output value=" /off/id "/>, with the following notes: <output value=" /off/r "/></value></text>
          <text id="/off/id:label">
            <value>Pasien ID</value>
          </text>
        </translation>
        <translation lang="हिन्दी (Hindi)">
          <text id="/off/r:label">
            <value>Notes</value>
          </text>
          <text id="/off/review:label">
            <value>Submitting report to turn OFF notifications for ID <output value=" /off/id "/>, with the following notes: <output value=" /off/r "/></value></text>
          <text id="/off/id:label">
            <value>मरीज का ID</value>
          </text>
        </translation>
        <translation default="true()" lang="English">
          <text id="/off/r:label">
            <value>Notes</value>
          </text>
          <text id="/off/review:label">
            <value>Submitting report to turn OFF notifications for ID <output value=" /off/id "/>, with the following notes: <output value=" /off/r "/></value></text>
          <text id="/off/id:label">
            <value>Patient ID</value>
          </text>
        </translation>
      </itext>
      <instance>
        <off delimiter="#" id="off" prefix="J1!off!" version="2017-08-12_07-01">
          <id/>
          <r/>
          <review/>
          <meta>
            <instanceID/>
          </meta>
        </off>
      </instance>
      <bind nodeset="/off/id" required="true()" type="string"/>
      <bind nodeset="/off/r" type="string"/>
      <bind nodeset="/off/review" readonly="true()" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/off/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body class="pages">
    <input appearance="numbers" ref="/off/id">
      <label ref="jr:itext('/off/id:label')"/>
    </input>
    <input ref="/off/r">
      <label ref="jr:itext('/off/r:label')"/>
    </input>
    <input ref="/off/review">
      <label ref="jr:itext('/off/review:label')"/>
    </input>
  </h:body>
</h:html>
