        </input>
        <input ref="/new_person_user_create/new_contact/contact_details/contact_phone">
          <label ref="jr:itext('/new_person_user_create/new_contact/contact_details/contact_phone:label')"/>
        </input>
        <select1 ref="/new_person_user_create/new_contact/contact_details/contact_role">
          <label ref="jr:itext('/new_person_user_create/new_contact/contact_details/contact_role:label')"/>
          <item>
            <label ref="jr:itext('/new_person_user_create/new_contact/contact_details/contact_role/chw:label')"/>
            <value>chw</value>
          </item>
          <item>
            <label ref="jr:itext('/new_person_user_create/new_contact/contact_details/contact_role/chw_supervisor:label')"/>
            <value>chw_supervisor</value>
          </item>
          <item>
            <label ref="jr:itext('/new_person_user_create/new_contact/contact_details/contact_role/nurse:label')"/>
            <value>nurse</value>
          </item>
          <item>
            <label ref="jr:itext('/new_person_user_create/new_contact/contact_details/contact_role/manager:label')"/>
            <value>manager</value>
          </item>
          <item>
            <label ref="jr:itext('/new_person_user_create/new_contact/contact_details/contact_role/patient:label')"/>
            <value>patient</value>
          </item>
        </select1>
        <group ref="/new_person_user_create/new_contact/contact_details/contact_profile">
          <group appearance="hidden" ref="/new_person_user_create/new_contact/contact_details/contact_profile/parent">
            <group appearance="hidden" ref="/new_person_user_create/new_contact/contact_details/contact_profile/parent/parent">
              <group appearance="hidden" ref="/new_person_user_create/new_contact/contact_details/contact_profile/parent/parent/parent"/>
            </group>
          </group>
          <group ref="/new_person_user_create/new_contact/contact_details/contact_profile/user_for_contact"/>
        </group>
      </group>
    </group>
  </h:body>
