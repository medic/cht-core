      <instance>
        <new_person_user_create delimiter="#" id="new_person_user_create" prefix="J1!new_person_user_create!" version="2023-01-25 15-50">
          <inputs>
            <meta>
              <location>
                <lat/>
                <long/>
                <error/>
                <message/>
              </location>
            </meta>
            <source>user</source>
            <source_id/>
            <user>
              <contact_id/>
              <name/>
              <phone/>
            </user>
            <contact>
              <_id/>
              <name/>
              <short_name/>
              <patient_id/>
              <date_of_birth>0</date_of_birth>
              <sex/>
              <parent>
                <_id/>
                <parent>
                  <_id/>
                  <parent>
                    <_id/>
                  </parent>
                  <contact>
                    <chw_name/>
                    <phone/>
                  </contact>
                </parent>
              </parent>
            </contact>
          </inputs>
          <household_id tag="hidden"/>
          <area_id tag="hidden"/>
          <facility_id tag="hidden"/>
          <patient_uuid tag="hidden"/>
          <patient_id tag="hidden"/>
          <patient_name tag="hidden"/>
          <t_danger_signs_referral_follow_up_date tag="hidden"/>
          <new_contact>
            <contact_details>
              <contact_name/>
              <contact_phone/>
              <contact_role/>
              <contact_profile db-doc="true">
                <name tag="hidden"/>
                <phone tag="hidden"/>
                <role tag="hidden"/>
                <parent tag="hidden">
                  <_id/>
                  <parent tag="hidden">
                    <_id/>
                    <parent tag="hidden">
                      <_id/>
                    </parent>
                  </parent>
                </parent>
                <type tag="hidden"/>
                <created_by_doc db-doc-ref="/new_person_user_create" tag="hidden"/>
                <user_for_contact>
                  <create/>
                </user_for_contact>
              </contact_profile>
              <child_doc db-doc-ref=" /new_person_user_create/new_contact/contact_details/contact_profile " tag="hidden"/>
            </contact_details>
          </new_contact>
          <meta tag="hidden">
            <instanceID/>
          </meta>
        </new_person_user_create>
      </instance>
      <instance id="contact-summary"/>
      <bind nodeset="/new_person_user_create/inputs" relevant="./source = 'user'"/>
      <bind nodeset="/new_person_user_create/inputs/source" type="string"/>
      <bind nodeset="/new_person_user_create/inputs/source_id" type="string"/>
      <bind nodeset="/new_person_user_create/inputs/user/contact_id" type="db:person"/>
      <bind nodeset="/new_person_user_create/inputs/user/name" type="string"/>
      <bind nodeset="/new_person_user_create/inputs/user/phone" type="string"/>
      <bind nodeset="/new_person_user_create/inputs/contact/_id" type="db:person"/>
      <bind nodeset="/new_person_user_create/inputs/contact/name" type="string"/>
      <bind nodeset="/new_person_user_create/inputs/contact/short_name" type="string"/>
      <bind nodeset="/new_person_user_create/inputs/contact/patient_id" type="string"/>
      <bind nodeset="/new_person_user_create/inputs/contact/date_of_birth" type="string"/>
      <bind nodeset="/new_person_user_create/inputs/contact/sex" type="string"/>
      <bind nodeset="/new_person_user_create/inputs/contact/parent/_id" type="string"/>
      <bind nodeset="/new_person_user_create/inputs/contact/parent/parent/_id" type="string"/>
      <bind nodeset="/new_person_user_create/inputs/contact/parent/parent/parent/_id" type="string"/>
      <bind nodeset="/new_person_user_create/inputs/contact/parent/parent/contact/chw_name" type="string"/>
      <bind nodeset="/new_person_user_create/inputs/contact/parent/parent/contact/phone" type="string"/>
      <bind calculate="../inputs/contact/parent/_id" nodeset="/new_person_user_create/household_id" type="string"/>
      <bind calculate="../inputs/contact/parent/parent/_id" nodeset="/new_person_user_create/area_id" type="string"/>
      <bind calculate="../inputs/contact/parent/parent/parent/_id" nodeset="/new_person_user_create/facility_id" type="string"/>
      <bind calculate="../inputs/contact/_id" nodeset="/new_person_user_create/patient_uuid" type="string"/>
      <bind calculate="../inputs/contact/patient_id" nodeset="/new_person_user_create/patient_id" type="string"/>
      <bind calculate="../inputs/contact/name" nodeset="/new_person_user_create/patient_name" type="string"/>
      <bind calculate="date-time(decimal-date-time(today()) + 3)" nodeset="/new_person_user_create/t_danger_signs_referral_follow_up_date" type="string"/>
      <bind nodeset="/new_person_user_create/new_contact/contact_details/contact_name" required="true()" type="string"/>
      <bind nodeset="/new_person_user_create/new_contact/contact_details/contact_phone" required="true()" type="string"/>
      <bind nodeset="/new_person_user_create/new_contact/contact_details/contact_role" required="true()" type="select1"/>
      <bind calculate="../../contact_name" nodeset="/new_person_user_create/new_contact/contact_details/contact_profile/name" type="string"/>
      <bind calculate="../../contact_phone" nodeset="/new_person_user_create/new_contact/contact_details/contact_profile/phone" type="string"/>
      <bind calculate="../../contact_role" nodeset="/new_person_user_create/new_contact/contact_details/contact_profile/role" type="string"/>
      <bind calculate=" /new_person_user_create/household_id " nodeset="/new_person_user_create/new_contact/contact_details/contact_profile/parent/_id" type="string"/>
      <bind calculate=" /new_person_user_create/area_id " nodeset="/new_person_user_create/new_contact/contact_details/contact_profile/parent/parent/_id" type="string"/>
      <bind calculate=" /new_person_user_create/facility_id " nodeset="/new_person_user_create/new_contact/contact_details/contact_profile/parent/parent/parent/_id" type="string"/>
      <bind calculate="&quot;person&quot;" nodeset="/new_person_user_create/new_contact/contact_details/contact_profile/type" type="string"/>
      <bind calculate="." nodeset="/new_person_user_create/new_contact/contact_details/contact_profile/created_by_doc" type="string"/>
      <bind calculate="selected(  /new_person_user_create/new_contact/contact_details/contact_profile/role ,'chw') or selected(  /new_person_user_create/new_contact/contact_details/contact_profile/role ,'chw_supervisor')" nodeset="/new_person_user_create/new_contact/contact_details/contact_profile/user_for_contact/create" type="string"/>
      <bind calculate=" /new_person_user_create/new_contact/contact_details " nodeset="/new_person_user_create/new_contact/contact_details/child_doc" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/new_person_user_create/meta/instanceID" readonly="true()" type="string"/>
    </model>
