  <h:body class="pages">
    <group appearance="field-list" ref="/new_person_user_create/inputs">
      <group ref="/new_person_user_create/inputs/user">
        <label ref="jr:itext('/new_person_user_create/inputs/user:label')"/>
        <input appearance="db-object" ref="/new_person_user_create/inputs/user/contact_id">
          <label ref="jr:itext('/new_person_user_create/inputs/user/contact_id:label')"/>
        </input>
      </group>
      <group ref="/new_person_user_create/inputs/contact">
        <input appearance="db-object" ref="/new_person_user_create/inputs/contact/_id">
          <label ref="jr:itext('/new_person_user_create/inputs/contact/_id:label')"/>
