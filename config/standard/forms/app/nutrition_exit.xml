      <bind nodeset="/nutrition_exit/inputs/contact/_id" type="db:person"/>
      <bind nodeset="/nutrition_exit/inputs/contact/name" type="string"/>
      <bind nodeset="/nutrition_exit/inputs/contact/patient_id" type="string"/>
      <bind nodeset="/nutrition_exit/inputs/contact/date_of_birth" type="string"/>
      <bind nodeset="/nutrition_exit/inputs/contact/parent/contact/phone" type="string"/>
      <bind nodeset="/nutrition_exit/inputs/contact/parent/contact/name" type="string"/>
      <bind nodeset="/nutrition_exit/patient_uuid" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/nutrition_exit/patient_name" type="string" calculate="../inputs/contact/name"/>
      <bind nodeset="/nutrition_exit/patient_id" type="string" calculate="../inputs/contact/patient_id"/>
      <bind nodeset="/nutrition_exit/enrollment_date" type="string" calculate="instance('contact-summary')/context/enrollment_date"/>
      <bind nodeset="/nutrition_exit/treatment_program" type="string" calculate="instance('contact-summary')/context/treatment_program"/>
      <bind nodeset="/nutrition_exit/wfh" type="string" calculate="round(z-score('weight-for-height',  /nutrition_exit/measurements/gender ,  /nutrition_exit/measurements/height ,  /nutrition_exit/measurements/weight ), 1)"/>
      <bind nodeset="/nutrition_exit/measurements/gender" type="select1" required="true()"/>
      <bind nodeset="/nutrition_exit/measurements/weight" type="decimal" required="true()" jr:constraintMsg="jr:itext('/nutrition_exit/measurements/weight:jr:constraintMsg')" constraint=". &gt;= 0.8 and . &lt;= 68.5"/>
      <bind nodeset="/nutrition_exit/measurements/height" type="decimal" required="true()" jr:constraintMsg="jr:itext('/nutrition_exit/measurements/height:jr:constraintMsg')" constraint=". &gt;= 45 and . &lt;= 120"/>
      <bind nodeset="/nutrition_exit/measurements/muac" type="decimal" required="false()" jr:constraintMsg="jr:itext('/nutrition_exit/measurements/muac:jr:constraintMsg')" constraint=". &gt;= 5 and . &lt;= 30"/>
      <bind nodeset="/nutrition_exit/measurements/n_wfh" readonly="true()" type="string" relevant=" /nutrition_exit/measurements/weight  != '' and  /nutrition_exit/measurements/height  != ''"/>
      <bind nodeset="/nutrition_exit/measurements/n_14" readonly="true()" type="string" relevant=" /nutrition_exit/wfh  &lt; -3"/>
      <bind nodeset="/nutrition_exit/measurements/n_15" readonly="true()" type="string" relevant=" /nutrition_exit/wfh  &lt; -2 and  /nutrition_exit/wfh  &gt;= -3"/>
      <bind nodeset="/nutrition_exit/measurements/n_16" readonly="true()" type="string" relevant=" /nutrition_exit/wfh  &lt;= 2 and  /nutrition_exit/wfh  &gt;= -2"/>
      <bind nodeset="/nutrition_exit/measurements/n_17" readonly="true()" type="string" relevant=" /nutrition_exit/wfh  &gt; 2"/>
      <bind nodeset="/nutrition_exit/exit/outcome" type="select1"/>
      <bind nodeset="/nutrition_exit/exit/enrollment" readonly="true()" type="string" relevant=" /nutrition_exit/exit/outcome  = 'cured'"/>
      <bind nodeset="/nutrition_exit/exit/length_stay" type="string" calculate="int(decimal-date-time(today()) - decimal-date-time(date( /nutrition_exit/enrollment_date )))"/>
      <bind nodeset="/nutrition_exit/exit/stay" readonly="true()" type="string" relevant=" /nutrition_exit/exit/outcome  = 'cured'"/>
      <bind nodeset="/nutrition_exit/exit/enroll" type="select1" relevant=" /nutrition_exit/exit/outcome  = 'cured'"/>
      <bind nodeset="/nutrition_exit/summary/n_2" readonly="true()" type="string"/>
      <bind nodeset="/nutrition_exit/summary/n_4" readonly="true()" type="string"/>
      <bind nodeset="/nutrition_exit/summary/n_6" readonly="true()" type="string"/>
      <bind nodeset="/nutrition_exit/summary/n_7" readonly="true()" type="string"/>
      <bind nodeset="/nutrition_exit/summary/n_9" readonly="true()" type="string"/>
      <bind nodeset="/nutrition_exit/summary/n_10" readonly="true()" type="string"/>
      <bind nodeset="/nutrition_exit/summary/n_11" readonly="true()" type="string" relevant=" /nutrition_exit/exit/outcome  = 'cured'"/>
      <bind nodeset="/nutrition_exit/summary/n_12" readonly="true()" type="string" relevant=" /nutrition_exit/exit/outcome  = 'cured'"/>
      <bind nodeset="/nutrition_exit/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
    </model>
