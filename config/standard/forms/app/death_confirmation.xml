      <instance>
        <death_confirmation id="death_confirmation" prefix="J1!death_confirmation!" delimiter="#" version="2022-09-26 12-31">
          <inputs>
            <meta>
              <location>
                <lat/>
                <long/>
                <error/>
                <message/>
              </location>
            </meta>
            <source>user</source>
            <source_id/>
            <contact>
              <_id/>
              <name/>
              <patient_id/>
              <date_of_birth/>
              <parent>
                <contact>
                  <phone/>
                  <name/>
                </contact>
              </parent>
            </contact>
          </inputs>
          <patient_uuid/>
          <child_name/>
          <patient_id/>
          <chw_name/>
          <chw_phone/>
          <death_report>
            <n_1/>
            <n_2/>
            <death/>
            <date_of_death/>
            <place/>
            <other_place/>
            <n_3/>
            <n_4/>
            <notes/>
          </death_report>
          <meta tag="hidden">
            <instanceID/>
          </meta>
        </death_confirmation>
      </instance>
      <instance id="contact-summary"/>
      <bind nodeset="/death_confirmation/inputs" relevant="./source = 'user'"/>
      <bind nodeset="/death_confirmation/inputs/source" type="string"/>
      <bind nodeset="/death_confirmation/inputs/source_id" type="string"/>
      <bind nodeset="/death_confirmation/inputs/contact/_id" type="db:person"/>
      <bind nodeset="/death_confirmation/inputs/contact/name" type="string"/>
      <bind nodeset="/death_confirmation/inputs/contact/patient_id" type="string"/>
      <bind nodeset="/death_confirmation/inputs/contact/date_of_birth" type="string"/>
      <bind nodeset="/death_confirmation/inputs/contact/parent/contact/phone" type="string"/>
      <bind nodeset="/death_confirmation/inputs/contact/parent/contact/name" type="string"/>
      <bind nodeset="/death_confirmation/patient_uuid" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/death_confirmation/child_name" type="string" calculate="../inputs/contact/name"/>
      <bind nodeset="/death_confirmation/patient_id" type="string" constraint="regex(., '[0-9]{5,13}')" calculate="../inputs/contact/patient_id"/>
      <bind nodeset="/death_confirmation/chw_name" type="string" calculate="../inputs/contact/parent/contact/name"/>
      <bind nodeset="/death_confirmation/chw_phone" type="string" calculate="../inputs/contact/parent/contact/phone"/>
      <bind nodeset="/death_confirmation/death_report/n_1" readonly="true()" type="string"/>
      <bind nodeset="/death_confirmation/death_report/n_2" readonly="true()" type="string"/>
      <bind nodeset="/death_confirmation/death_report/death" type="select1" required="true()"/>
      <bind nodeset="/death_confirmation/death_report/date_of_death" type="date" required="true()" relevant=" /death_confirmation/death_report/death  = 'yes'" constraint=". &lt;= now()"/>
      <bind nodeset="/death_confirmation/death_report/place" type="select1" required="true()" relevant=" /death_confirmation/death_report/death  = 'yes'"/>
      <bind nodeset="/death_confirmation/death_report/other_place" type="string" required="true()" relevant=" /death_confirmation/death_report/place  = 'other'"/>
      <bind nodeset="/death_confirmation/death_report/n_3" readonly="true()" type="string" relevant=" /death_confirmation/death_report/death  = 'no'"/>
      <bind nodeset="/death_confirmation/death_report/n_4" readonly="true()" type="string" relevant=" /death_confirmation/death_report/death  = 'yes'"/>
      <bind nodeset="/death_confirmation/death_report/notes" type="string"/>
      <bind nodeset="/death_confirmation/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
    </model>
  </h:head>
