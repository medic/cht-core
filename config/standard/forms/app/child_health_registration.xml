              <name/>
              <date_of_birth>0</date_of_birth>
              <sex/>
              <phone/>
              <parent>
                <contact>
                  <phone/>
                  <name/>
                </contact>
                <parent>
                  <parent>
                    <use_cases/>
                  </parent>
                </parent>
              </parent>
            </contact>
          </inputs>
          <patient_uuid tag="hidden"/>
          <patient_id/>
          <patient_name/>
          <chw_name/>
          <chw_phone/>
          <birth_date/>
          <chw_sms/>
          <geolocation tag="hidden"/>
          <patient_age_in_years tag="hidden">0</patient_age_in_years>
          <patient_contact_phone tag="hidden"/>
          <group_note tag="hidden">
            <default_chw_sms>default</default_chw_sms>
            <default_chw_sms_text/>
            <default_chw_sms_note/>
            <is_sms_edited>yes</is_sms_edited>
            <g_chw_sms/>
          </group_note>
          <group_summary tag="hidden">
            <submit/>
            <r_summary/>
            <r_patient_id/>
            <r_patient_info/>
            <r_followup/>
            <r_followup_note1/>
            <r_followup_note2/>
          </group_summary>
          <meta tag="hidden">
            <instanceID/>
          </meta>
        </child_health_registration>
      </instance>
      <instance id="contact-summary"/>
      <bind nodeset="/child_health_registration/inputs" relevant="./source = 'user'"/>
      <bind nodeset="/child_health_registration/inputs/source" type="string"/>
      <bind nodeset="/child_health_registration/inputs/source_id" type="string"/>
      <bind nodeset="/child_health_registration/inputs/contact/_id" required="true()" type="db:person"/>
      <bind nodeset="/child_health_registration/inputs/contact/patient_id" type="string"/>
      <bind nodeset="/child_health_registration/inputs/contact/name" type="string"/>
      <bind nodeset="/child_health_registration/inputs/contact/date_of_birth" type="string"/>
      <bind nodeset="/child_health_registration/inputs/contact/sex" type="string"/>
      <bind nodeset="/child_health_registration/inputs/contact/phone" type="string"/>
      <bind nodeset="/child_health_registration/inputs/contact/parent/contact/phone" type="string"/>
      <bind nodeset="/child_health_registration/inputs/contact/parent/contact/name" type="string"/>
      <bind nodeset="/child_health_registration/inputs/contact/parent/parent/parent/use_cases" type="string"/>
      <bind calculate="../inputs/contact/_id" nodeset="/child_health_registration/patient_uuid" type="string"/>
      <bind calculate="../inputs/contact/patient_id" nodeset="/child_health_registration/patient_id" type="string"/>
      <bind calculate="../inputs/contact/name" nodeset="/child_health_registration/patient_name" type="string"/>
      <bind calculate="../inputs/contact/parent/contact/name" nodeset="/child_health_registration/chw_name" type="string"/>
      <bind calculate="../inputs/contact/parent/contact/phone" nodeset="/child_health_registration/chw_phone" type="string"/>
      <bind calculate="substr(  /child_health_registration/inputs/contact/date_of_birth , 0, 10 )" nodeset="/child_health_registration/birth_date" type="string"/>
      <bind calculate="if( /child_health_registration/group_note/g_chw_sms  != '', concat( /child_health_registration/group_note/default_chw_sms_text ,concat('
', /child_health_registration/group_note/g_chw_sms )),  /child_health_registration/group_note/default_chw_sms_text )" nodeset="/child_health_registration/chw_sms" type="string"/>
      <bind calculate="concat(../../inputs/meta/location/lat, concat(' ', ../../inputs/meta/location/long))" nodeset="/child_health_registration/geolocation" type="string"/>
      <bind calculate="floor( difference-in-months(  /child_health_registration/birth_date , today() ) div 12 )" nodeset="/child_health_registration/patient_age_in_years" type="string"/>
      <bind calculate="coalesce(/postnatal_care/inputs/contact/phone,/postnatal_care/inputs/contact/parent/contact/phone)" nodeset="/child_health_registration/patient_contact_phone" type="string"/>
      <bind nodeset="/child_health_registration/group_note/default_chw_sms" type="select1"/>
      <bind calculate="jr:choice-name( /child_health_registration/group_note/default_chw_sms ,' /child_health_registration/group_note/default_chw_sms ')" nodeset="/child_health_registration/group_note/default_chw_sms_text" type="string"/>
      <bind nodeset="/child_health_registration/group_note/default_chw_sms_note" readonly="true()" type="string"/>
      <bind nodeset="/child_health_registration/group_note/is_sms_edited" required="true()" type="select1"/>
      <bind constraint="string-length(.) &lt;= 715" jr:constraintMsg="jr:itext('/child_health_registration/group_note/g_chw_sms:jr:constraintMsg')" nodeset="/child_health_registration/group_note/g_chw_sms" type="string"/>
      <bind nodeset="/child_health_registration/group_summary/submit" readonly="true()" type="string"/>
      <bind nodeset="/child_health_registration/group_summary/r_summary" readonly="true()" type="string"/>
      <bind calculate="../../inputs/contact/patient_id" nodeset="/child_health_registration/group_summary/r_patient_id" type="string"/>
      <bind nodeset="/child_health_registration/group_summary/r_patient_info" readonly="true()" type="string"/>
      <bind nodeset="/child_health_registration/group_summary/r_followup" readonly="true()" relevant=" /child_health_registration/chw_sms  != ''" type="string"/>
      <bind nodeset="/child_health_registration/group_summary/r_followup_note1" readonly="true()" relevant=" /child_health_registration/chw_sms  != ''" type="string"/>
      <bind nodeset="/child_health_registration/group_summary/r_followup_note2" readonly="true()" relevant=" /child_health_registration/chw_sms  != ''" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/child_health_registration/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body class="pages">
    <group appearance="field-list" ref="/child_health_registration/inputs">
      <label ref="jr:itext('/child_health_registration/inputs:label')"/>
      <group ref="/child_health_registration/inputs/contact">
        <input appearance="db-object" ref="/child_health_registration/inputs/contact/_id">
          <label ref="jr:itext('/child_health_registration/inputs/contact/_id:label')"/>
          <hint ref="jr:itext('/child_health_registration/inputs/contact/_id:hint')"/>
        </input>
        <input appearance="hidden" ref="/child_health_registration/inputs/contact/patient_id">
          <label ref="jr:itext('/child_health_registration/inputs/contact/patient_id:label')"/>
        </input>
        <input appearance="hidden" ref="/child_health_registration/inputs/contact/name">
          <label ref="jr:itext('/child_health_registration/inputs/contact/name:label')"/>
        </input>
        <input appearance="hidden" ref="/child_health_registration/inputs/contact/date_of_birth">
          <label ref="jr:itext('/child_health_registration/inputs/contact/date_of_birth:label')"/>
        </input>
        <input appearance="hidden" ref="/child_health_registration/inputs/contact/sex">
          <label ref="jr:itext('/child_health_registration/inputs/contact/sex:label')"/>
        </input>
        <input appearance="hidden" ref="/child_health_registration/inputs/contact/phone">
          <label ref="jr:itext('/child_health_registration/inputs/contact/phone:label')"/>
        </input>
        <group ref="/child_health_registration/inputs/contact/parent">
          <group ref="/child_health_registration/inputs/contact/parent/contact">
            <input appearance="hidden" ref="/child_health_registration/inputs/contact/parent/contact/phone">
              <label ref="jr:itext('/child_health_registration/inputs/contact/parent/contact/phone:label')"/>
            </input>
            <input appearance="hidden" ref="/child_health_registration/inputs/contact/parent/contact/name">
              <label ref="jr:itext('/child_health_registration/inputs/contact/parent/contact/name:label')"/>
            </input>
          </group>
          <group ref="/child_health_registration/inputs/contact/parent/parent">
            <group ref="/child_health_registration/inputs/contact/parent/parent/parent">
              <input appearance="hidden" ref="/child_health_registration/inputs/contact/parent/parent/parent/use_cases">
                <label ref="jr:itext('/child_health_registration/inputs/contact/parent/parent/parent/use_cases:label')"/>
              </input>
            </group>
          </group>
        </group>
      </group>
    </group>
    <group appearance="field-list" ref="/child_health_registration/group_note">
      <label ref="jr:itext('/child_health_registration/group_note:label')"/>
      <select1 appearance="hidden" ref="/child_health_registration/group_note/default_chw_sms">
        <label ref="jr:itext('/child_health_registration/group_note/default_chw_sms:label')"/>
        <item>
          <label ref="jr:itext('/child_health_registration/group_note/default_chw_sms/default:label')"/>
          <value>default</value>
        </item>
      </select1>
      <input ref="/child_health_registration/group_note/default_chw_sms_note">
        <label ref="jr:itext('/child_health_registration/group_note/default_chw_sms_note:label')"/>
      </input>
      <select1 appearance="hidden" ref="/child_health_registration/group_note/is_sms_edited">
        <label ref="jr:itext('/child_health_registration/group_note/is_sms_edited:label')"/>
        <item>
          <label ref="jr:itext('/child_health_registration/group_note/is_sms_edited/yes:label')"/>
          <value>yes</value>
        </item>
        <item>
          <label ref="jr:itext('/child_health_registration/group_note/is_sms_edited/no:label')"/>
          <value>no</value>
        </item>
      </select1>
      <input appearance="multiline" ref="/child_health_registration/group_note/g_chw_sms">
        <label ref="jr:itext('/child_health_registration/group_note/g_chw_sms:label')"/>
        <hint ref="jr:itext('/child_health_registration/group_note/g_chw_sms:hint')"/>
      </input>
    </group>
    <group appearance="field-list summary" ref="/child_health_registration/group_summary">
      <input ref="/child_health_registration/group_summary/submit">
        <label ref="jr:itext('/child_health_registration/group_summary/submit:label')"/>
      </input>
      <input appearance="h1 yellow" ref="/child_health_registration/group_summary/r_summary">
        <label ref="jr:itext('/child_health_registration/group_summary/r_summary:label')"/>
      </input>
      <input appearance="h4 center" ref="/child_health_registration/group_summary/r_patient_info">
        <label ref="jr:itext('/child_health_registration/group_summary/r_patient_info:label')"/>
      </input>
      <input appearance="h1 green" ref="/child_health_registration/group_summary/r_followup">
        <label ref="jr:itext('/child_health_registration/group_summary/r_followup:label')"/>
      </input>
      <input ref="/child_health_registration/group_summary/r_followup_note1">
        <label ref="jr:itext('/child_health_registration/group_summary/r_followup_note1:label')"/>
      </input>
      <input appearance="li" ref="/child_health_registration/group_summary/r_followup_note2">
        <label ref="jr:itext('/child_health_registration/group_summary/r_followup_note2:label')"/>
      </input>
    </group>
  </h:body>
</h:html>
