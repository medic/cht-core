      <instance>
        <off id="off" prefix="J1!off!" delimiter="#" version="2018-11-14_10-16">
          <inputs>
            <meta>
              <location>
                <lat/>
                <long/>
                <error/>
                <message/>
              </location>
            </meta>
            <source>user</source>
            <source_id/>
            <contact>
              <_id/>
              <patient_id/>
              <name/>
              <date_of_birth/>
              <sex/>
              <parent>
                <contact>
                  <phone/>
                  <name/>
                </contact>
              </parent>
            </contact>
          </inputs>
          <patient_uuid/>
          <patient_id/>
          <patient_name/>
          <chw_name/>
          <chw_phone/>
          <chw_sms/>
          <group_note>
            <pregnancy_note/>
            <send_note/>
            <chw_note/>
          </group_note>
          <meta tag="hidden">
            <instanceID/>
          </meta>
        </off>
      </instance>
      <instance id="contact-summary"/>
      <bind nodeset="/off/inputs" relevant="./source = 'user'"/>
      <bind nodeset="/off/inputs/source" type="string"/>
      <bind nodeset="/off/inputs/source_id" type="string"/>
      <bind nodeset="/off/inputs/contact/_id" type="db:person"/>
      <bind nodeset="/off/inputs/contact/patient_id" type="string"/>
      <bind nodeset="/off/inputs/contact/name" type="string"/>
      <bind nodeset="/off/inputs/contact/date_of_birth" type="string"/>
      <bind nodeset="/off/inputs/contact/sex" type="string"/>
      <bind nodeset="/off/inputs/contact/parent/contact/phone" type="string"/>
      <bind nodeset="/off/inputs/contact/parent/contact/name" type="string"/>
      <bind nodeset="/off/patient_uuid" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/off/patient_id" type="string" calculate="../inputs/contact/patient_id"/>
      <bind nodeset="/off/patient_name" type="string" calculate="../inputs/contact/name"/>
      <bind nodeset="/off/chw_name" type="string" calculate="../inputs/contact/parent/contact/name"/>
      <bind nodeset="/off/chw_phone" type="string" calculate="../inputs/contact/parent/contact/phone"/>
      <bind nodeset="/off/chw_sms" type="string" calculate=" /off/group_note/chw_note "/>
      <bind nodeset="/off/group_note/pregnancy_note" readonly="true()" type="string"/>
      <bind nodeset="/off/group_note/send_note" type="select1" required="true()"/>
      <bind nodeset="/off/group_note/chw_note" type="string" relevant=" /off/group_note/send_note  = 'yes'"/>
      <bind nodeset="/off/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
    </model>
  </h:head>
