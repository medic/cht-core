          <treatment_program/>
          <wfh/>
          <measurements>
            <gender/>
            <weight/>
            <height/>
            <muac/>
            <nn3/>
            <n_14/>
            <n_15/>
            <n_16/>
            <n_17/>
            <exit/>
            <n/>
