      <instance id="contact-summary"/>
      <bind nodeset="/nutrition_followup/inputs" relevant="./source = 'user'"/>
      <bind nodeset="/nutrition_followup/inputs/source" type="string"/>
      <bind nodeset="/nutrition_followup/inputs/source_id" type="string"/>
      <bind nodeset="/nutrition_followup/inputs/contact/_id" type="db:person"/>
      <bind nodeset="/nutrition_followup/inputs/contact/name" type="string"/>
      <bind nodeset="/nutrition_followup/inputs/contact/patient_id" type="string"/>
      <bind nodeset="/nutrition_followup/inputs/contact/date_of_birth" type="string"/>
      <bind nodeset="/nutrition_followup/inputs/contact/parent/contact/phone" type="string"/>
      <bind nodeset="/nutrition_followup/inputs/contact/parent/contact/name" type="string"/>
      <bind nodeset="/nutrition_followup/patient_uuid" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/nutrition_followup/patient_name" type="string" calculate="../inputs/contact/name"/>
      <bind nodeset="/nutrition_followup/patient_id" type="string" calculate="../inputs/contact/patient_id"/>
      <bind nodeset="/nutrition_followup/attend_visit" type="select1" relevant=" /nutrition_followup/inputs/source  = 'task'"/>
      <bind nodeset="/nutrition_followup/treatment_program" type="string" calculate="instance('contact-summary')/context/treatment_program"/>
      <bind nodeset="/nutrition_followup/wfh" type="string" calculate="round(z-score('weight-for-height',  /nutrition_followup/measurements/gender ,  /nutrition_followup/measurements/height ,  /nutrition_followup/measurements/weight ), 1)"/>
      <bind nodeset="/nutrition_followup/measurements" relevant=" /nutrition_followup/attend_visit  = 'yes'"/>
      <bind nodeset="/nutrition_followup/measurements/gender" type="select1" required="true()"/>
      <bind nodeset="/nutrition_followup/measurements/weight" type="decimal" constraint=". &gt;= 0.8 and . &lt;= 68.5" jr:constraintMsg="jr:itext('/nutrition_followup/measurements/weight:jr:constraintMsg')" required="true()"/>
      <bind nodeset="/nutrition_followup/measurements/height" type="decimal" constraint=". &gt;= 45 and . &lt;= 120" jr:constraintMsg="jr:itext('/nutrition_followup/measurements/height:jr:constraintMsg')" required="true()"/>
      <bind nodeset="/nutrition_followup/measurements/muac" type="decimal" constraint=". &gt;= 5 and . &lt;= 30" jr:constraintMsg="jr:itext('/nutrition_followup/measurements/muac:jr:constraintMsg')" required="false()"/>
      <bind nodeset="/nutrition_followup/measurements/nn3" readonly="true()" type="string" relevant=" /nutrition_followup/measurements/weight  != '' and  /nutrition_followup/measurements/height  != ''"/>
      <bind nodeset="/nutrition_followup/measurements/n_14" readonly="true()" type="string" relevant=" /nutrition_followup/wfh  &lt; -3"/>
      <bind nodeset="/nutrition_followup/measurements/n_15" readonly="true()" type="string" relevant=" /nutrition_followup/wfh  &lt; -2 and  /nutrition_followup/wfh  &gt;= -3"/>
      <bind nodeset="/nutrition_followup/measurements/n_16" readonly="true()" type="string" relevant=" /nutrition_followup/wfh  &lt;= 2 and  /nutrition_followup/wfh  &gt;= -2"/>
      <bind nodeset="/nutrition_followup/measurements/n_17" readonly="true()" type="string" relevant=" /nutrition_followup/wfh  &gt; 2"/>
      <bind nodeset="/nutrition_followup/measurements/exit" type="select1" required="true()"/>
      <bind nodeset="/nutrition_followup/measurements/n" readonly="true()" type="string" relevant=" /nutrition_followup/measurements/exit  = 'yes'"/>
      <bind nodeset="/nutrition_followup/summary" relevant=" /nutrition_followup/attend_visit  = 'yes'"/>
      <bind nodeset="/nutrition_followup/summary/n_1" readonly="true()" type="string"/>
      <bind nodeset="/nutrition_followup/summary/n_2" readonly="true()" type="string"/>
      <bind nodeset="/nutrition_followup/summary/n_3" readonly="true()" type="string"/>
      <bind nodeset="/nutrition_followup/summary/n_4" readonly="true()" type="string"/>
      <bind nodeset="/nutrition_followup/summary/n_5" readonly="true()" type="string"/>
      <bind nodeset="/nutrition_followup/summary/n_6" readonly="true()" type="string"/>
      <bind nodeset="/nutrition_followup/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
    </model>
  </h:head>
