      <bind nodeset="/data/data/__visited_hf" type="string" calculate=" /data/danger_signs/visit_confirm "></bind>
      <bind nodeset="/data/data/__still_experiencing_danger_sign" type="string" calculate=" /data/danger_signs/danger_sign_present "></bind>
      <bind nodeset="/data/data/__fever" type="string" calculate=" /data/danger_signs/fever "></bind>
