      <bind calculate="date-time(floor(decimal-date-time(today())) + 3)" nodeset="/pnc_danger_sign_follow_up_mother/t_danger_signs_referral_follow_up_date" type="string"/>
      <bind calculate="../danger_signs/r_danger_sign_present" nodeset="/pnc_danger_sign_follow_up_mother/t_danger_signs_referral_follow_up" type="string"/>
      <bind nodeset="/pnc_danger_sign_follow_up_mother/danger_signs/visit_confirm" required="true()" type="select1"/>
      <bind nodeset="/pnc_danger_sign_follow_up_mother/danger_signs/danger_sign_present" required="true()" type="select1"/>
      <bind nodeset="/pnc_danger_sign_follow_up_mother/danger_signs/danger_signs_question_note" readonly="true()" relevant="../danger_sign_present = 'yes' or ../danger_sign_present = 'no'" type="string"/>
      <bind nodeset="/pnc_danger_sign_follow_up_mother/danger_signs/fever" relevant="../danger_sign_present = 'yes' or ../danger_sign_present = 'no'" required="true()" type="select1"/>
      <bind nodeset="/pnc_danger_sign_follow_up_mother/danger_signs/severe_headache" relevant="../danger_sign_present = 'yes' or ../danger_sign_present = 'no'" required="true()" type="select1"/>
      <bind nodeset="/pnc_danger_sign_follow_up_mother/danger_signs/vaginal_bleeding" relevant="../danger_sign_present = 'yes' or ../danger_sign_present = 'no'" required="true()" type="select1"/>
      <bind nodeset="/pnc_danger_sign_follow_up_mother/danger_signs/vaginal_discharge" relevant="../danger_sign_present = 'yes' or ../danger_sign_present = 'no'" required="true()" type="select1"/>
      <bind nodeset="/pnc_danger_sign_follow_up_mother/danger_signs/convulsion" relevant="../danger_sign_present = 'yes' or ../danger_sign_present = 'no'" required="true()" type="select1"/>
      <bind calculate="if(selected(../fever, 'yes') or
selected(../severe_headache, 'yes') or
selected(../vaginal_bleeding, 'yes') or
selected(../vaginal_discharge, 'yes') or
selected(../convulsion, 'yes'), 'yes',
if(selected(../severe_headache, 'no') and
selected(../vaginal_bleeding, 'no') and
selected(../vaginal_discharge, 'no') and
selected(../convulsion, 'no'), 'no', ''))" nodeset="/pnc_danger_sign_follow_up_mother/danger_signs/r_danger_sign_present" type="string"/>
      <bind nodeset="/pnc_danger_sign_follow_up_mother/danger_signs/congratulate_no_ds_note" readonly="true()" relevant="../danger_sign_present = 'no' and ../r_danger_sign_present = 'no'" type="string"/>
      <bind nodeset="/pnc_danger_sign_follow_up_mother/danger_signs/refer_patient_note_1" readonly="true()" relevant="../danger_sign_present = 'yes' or ../r_danger_sign_present = 'yes'" type="string"/>
      <bind nodeset="/pnc_danger_sign_follow_up_mother/danger_signs/refer_patient_note_2" readonly="true()" relevant="../danger_sign_present = 'yes' or ../r_danger_sign_present = 'yes'" type="string"/>
      <bind calculate="&quot;woman&quot;" nodeset="/pnc_danger_sign_follow_up_mother/danger_signs/custom_translations/custom_woman_label_translator" type="select1"/>
      <bind calculate="jr:choice-name( /pnc_danger_sign_follow_up_mother/danger_signs/custom_translations/custom_woman_label_translator ,' /pnc_danger_sign_follow_up_mother/danger_signs/custom_translations/custom_woman_label_translator ')" nodeset="/pnc_danger_sign_follow_up_mother/danger_signs/custom_translations/custom_woman_label" type="string"/>
      <bind calculate="&quot;woman-start&quot;" nodeset="/pnc_danger_sign_follow_up_mother/danger_signs/custom_translations/custom_woman_start_label_translator" type="select1"/>
      <bind calculate="jr:choice-name( /pnc_danger_sign_follow_up_mother/danger_signs/custom_translations/custom_woman_start_label_translator ,' /pnc_danger_sign_follow_up_mother/danger_signs/custom_translations/custom_woman_start_label_translator ')" nodeset="/pnc_danger_sign_follow_up_mother/danger_signs/custom_translations/custom_woman_start_label" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/pnc_danger_sign_follow_up_mother/meta/instanceID" readonly="true()" type="string"/>
    </model>
