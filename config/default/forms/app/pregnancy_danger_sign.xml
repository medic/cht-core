      <bind nodeset="/data/danger_signs/severe_abdominal_pain" type="string" required="true()"></bind>
      <bind nodeset="/data/danger_signs/severe_headache" type="string" required="true()"></bind>
      <bind nodeset="/data/danger_signs/very_pale" type="string" required="true()"></bind>
      <bind nodeset="/data/danger_signs/fever" type="string" required="true()"></bind>
      <bind nodeset="/data/danger_signs/reduced_or_no_fetal_movements" type="string" required="true()"></bind>
      <bind nodeset="/data/danger_signs/breaking_water" type="string" required="true()"></bind>
      <bind nodeset="/data/danger_signs/easily_tired" type="string" required="true()"></bind>
      <bind nodeset="/data/danger_signs/face_hand_swelling" type="string" required="true()"></bind>
      <bind nodeset="/data/danger_signs/breathlessness" type="string" required="true()"></bind>
      <bind nodeset="/data/danger_signs/r_danger_sign_present" type="string" calculate="if(selected(../vaginal_bleeding, 'yes')&#10;or selected(../fits, 'yes')&#10;or selected(../severe_abdominal_pain, 'yes')&#10;or selected(../severe_headache, 'yes')&#10;or selected(../very_pale, 'yes')&#10;or selected(../fever, 'yes')&#10;or selected(../reduced_or_no_fetal_movements, 'yes')&#10;or selected(../breaking_water, 'yes')&#10;or selected(../easily_tired, 'yes')&#10;or selected(../face_hand_swelling, 'yes')&#10;or selected(../breathlessness, 'yes'), &#10;'yes',&#10;if(selected(../vaginal_bleeding, 'no')&#10;and selected(../fits, 'no')&#10;and selected(../severe_abdominal_pain, 'no')&#10;and selected(../severe_headache, 'no')&#10;and selected(../very_pale, 'no')&#10;and selected(../fever, 'no')&#10;and selected(../reduced_or_no_fetal_movements, 'no')&#10;and selected(../breaking_water, 'no')&#10;and selected(../easily_tired, 'no')&#10;and selected(../face_hand_swelling, 'no')&#10;and selected(../breathlessness, 'no'), 'no', ''))"></bind>
      <bind nodeset="/data/danger_signs/congratulate_no_ds_note" readonly="true()" type="string" relevant="../r_danger_sign_present = 'no'"></bind>
      <bind nodeset="/data/danger_signs/refer_patient_note_1" readonly="true()" type="string" relevant="../r_danger_sign_present = 'yes'"></bind>
      <bind nodeset="/data/danger_signs/refer_patient_note_2" readonly="true()" type="string" relevant="../r_danger_sign_present = 'yes'"></bind>
      <bind nodeset="/data/danger_signs/custom_translations/custom_woman_label_translator" type="string" calculate="&quot;woman&quot;"></bind>
      <bind nodeset="/data/danger_signs/custom_translations/custom_woman_label" type="string" calculate="jr:choice-name( /data/danger_signs/custom_translations/custom_woman_label_translator ,' /data/danger_signs/custom_translations/custom_woman_label_translator ')"></bind>
      <bind nodeset="/data/danger_signs/custom_translations/custom_woman_start_label_translator" type="string" calculate="&quot;woman-start&quot;"></bind>
      <bind nodeset="/data/danger_signs/custom_translations/custom_woman_start_label" type="string" calculate="jr:choice-name( /data/danger_signs/custom_translations/custom_woman_start_label_translator ,' /data/danger_signs/custom_translations/custom_woman_start_label_translator ')"></bind>
      <bind nodeset="/data/data/__vaginal_bleeding" type="string" calculate=" /data/danger_signs/vaginal_bleeding "></bind>
      <bind nodeset="/data/data/__fits" type="string" calculate=" /data/danger_signs/fits "></bind>
      <bind nodeset="/data/data/__severe_abdominal_pain" type="string" calculate=" /data/danger_signs/severe_abdominal_pain "></bind>
      <bind nodeset="/data/data/__severe_headache" type="string" calculate=" /data/danger_signs/severe_headache "></bind>
      <bind nodeset="/data/data/__very_pale" type="string" calculate=" /data/danger_signs/very_pale "></bind>
      <bind nodeset="/data/data/__fever" type="string" calculate=" /data/danger_signs/fever "></bind>
      <bind nodeset="/data/data/__reduced_or_no_fetal_movements" type="string" calculate=" /data/danger_signs/reduced_or_no_fetal_movements "></bind>
      <bind nodeset="/data/data/__breaking_water" type="string" calculate=" /data/danger_signs/breaking_water "></bind>
      <bind nodeset="/data/data/__easily_tired" type="string" calculate=" /data/danger_signs/easily_tired "></bind>
      <bind nodeset="/data/data/__face_hand_swelling" type="string" calculate=" /data/danger_signs/face_hand_swelling "></bind>
      <bind nodeset="/data/data/__breathlessness" type="string" calculate=" /data/danger_signs/breathlessness "></bind>
      <bind nodeset="/data/data/__has_danger_sign" type="string" calculate=" /data/danger_signs/r_danger_sign_present "></bind>
      <bind nodeset="/data/data/meta/__patient_uuid" type="string" calculate="../../../inputs/contact/_id"></bind>
      <bind nodeset="/data/data/meta/__patient_id" type="string" calculate="../../../inputs/contact/patient_id"></bind>
      <bind nodeset="/data/data/meta/__household_uuid" type="string" calculate="../../../inputs/contact/parent/_id"></bind>
      <bind nodeset="/data/data/meta/__source" type="string" calculate="../../../inputs/source"></bind>
      <bind nodeset="/data/data/meta/__source_id" type="string" calculate="../../../inputs/source_id"></bind>
      <bind nodeset="/data/data/meta/__pregnancy_uuid" type="string" calculate=" /data/pregnancy_uuid_ctx "></bind>
      <bind nodeset="/data/meta/instanceID" type="string" readonly="true()" jr:preload="uid"></bind>
    </model>
