      <bind nodeset="/data/patient_id" type="string" calculate="../inputs/contact/patient_id"></bind>
      <bind nodeset="/data/patient_name" type="string" calculate="../inputs/contact/name"></bind>
      <bind nodeset="/data/patient_short_name" type="string" calculate="coalesce(../inputs/contact/short_name, ../danger_signs/custom_translations/custom_woman_label)"></bind>
      <bind nodeset="/data/patient_short_name_start" type="string" calculate="coalesce(../inputs/contact/short_name, ../danger_signs/custom_translations/custom_woman_start_label)"></bind>
      <bind nodeset="/data/t_danger_signs_referral_follow_up_date" type="string" calculate="date-time(decimal-date-time(today()) + 3)"></bind>
      <bind nodeset="/data/t_danger_signs_referral_follow_up" type="string" calculate="../danger_signs/r_danger_sign_present"></bind>
      <bind nodeset="/data/pregnancy_uuid_ctx" type="string" calculate="if(instance('contact-summary')/context/pregnancy_uuid != '', instance('contact-summary')/context/pregnancy_uuid, .)"></bind>
      <bind nodeset="/data/danger_signs/danger_signs_note" readonly="true()" type="string"></bind>
      <bind nodeset="/data/danger_signs/danger_signs_question_note" readonly="true()" type="string"></bind>
      <bind nodeset="/data/danger_signs/vaginal_bleeding" type="string" required="true()"></bind>
