      <bind nodeset="/data/parent_3_id" type="string" calculate="../inputs/contact/parent/parent/parent/_id"></bind>
      <bind nodeset="/data/user_phone" type="string" calculate="../inputs/user/phone"></bind>
      <bind nodeset="/data/contact_role" type="string" calculate="../inputs/contact/role"></bind>
      <bind nodeset="/data/created_by" type="string" calculate="../inputs/user/name"></bind>
      <bind nodeset="/data/created_by_person_uuid" type="string" calculate="../inputs/user/contact_id"></bind>
      <bind nodeset="/data/created_by_place_uuid" type="string" calculate="../inputs/user/facility_id"></bind>
      <bind nodeset="/data/intro/warning_header" readonly="true()" type="string"></bind>
      <bind nodeset="/data/intro/warning" readonly="true()" type="string"></bind>
      <bind nodeset="/data/intro/admin_code" type="string" required="true()" constraint=". = '1234'" jr:constraintMsg="jr:itext('/data/intro/admin_code:jr:constraintMsg')"></bind>
      <bind nodeset="/data/new_contact/parent/_id" type="string" calculate=" /data/parent_1_id "></bind>
      <bind nodeset="/data/new_contact/parent/parent" relevant=" /data/parent_2_id  != ''"></bind>
      <bind nodeset="/data/new_contact/parent/parent/_id" type="string" calculate=" /data/parent_2_id "></bind>
      <bind nodeset="/data/new_contact/parent/parent/parent" relevant=" /data/parent_3_id  != ''"></bind>
      <bind nodeset="/data/new_contact/parent/parent/parent/_id" type="string" calculate=" /data/parent_3_id "></bind>
      <bind nodeset="/data/new_contact/type" type="string" calculate="&quot;person&quot;"></bind>
      <bind nodeset="/data/new_contact/name" type="string" required="true()"></bind>
      <bind nodeset="/data/new_contact/short_name" type="string" constraint="string-length(.) &lt;= 10" jr:constraintMsg="jr:itext('/data/new_contact/short_name:jr:constraintMsg')"></bind>
      <bind nodeset="/data/new_contact/date_of_birth" type="string" required="true()" calculate=" /data/new_contact/ephemeral_dob/dob_iso "></bind>
      <bind nodeset="/data/new_contact/date_of_birth_method" type="string" calculate=" /data/new_contact/ephemeral_dob/dob_method "></bind>
      <bind nodeset="/data/new_contact/ephemeral_dob/dob_calendar" type="date" required="true()" relevant="not(selected( /data/new_contact/ephemeral_dob/dob_method ,'approx'))" constraint="floor(decimal-date-time(.)) &lt;= floor(decimal-date-time(today()))" jr:constraintMsg="jr:itext('/data/new_contact/ephemeral_dob/dob_calendar:jr:constraintMsg')"></bind>
      <bind nodeset="/data/new_contact/ephemeral_dob/age_label" readonly="true()" type="string" relevant="selected( /data/new_contact/ephemeral_dob/dob_method ,'approx')"></bind>
      <bind nodeset="/data/new_contact/ephemeral_dob/age_years" type="int" required="true()" relevant="selected( /data/new_contact/ephemeral_dob/dob_method ,'approx')" constraint=". &gt;= 0 and . &lt;= 130" jr:constraintMsg="jr:itext('/data/new_contact/ephemeral_dob/age_years:jr:constraintMsg')"></bind>
      <bind nodeset="/data/new_contact/ephemeral_dob/age_months" type="int" relevant="selected( /data/new_contact/ephemeral_dob/dob_method ,'approx')" constraint=". &gt;= 0 and . &lt;= 11" jr:constraintMsg="jr:itext('/data/new_contact/ephemeral_dob/age_months:jr:constraintMsg')"></bind>
      <bind nodeset="/data/new_contact/ephemeral_dob/dob_method" type="string"></bind>
      <bind nodeset="/data/new_contact/ephemeral_dob/ephemeral_months" type="string" calculate="if(format-date-time(today(),&quot;%m&quot;) -  /data/new_contact/ephemeral_dob/age_months  &lt; 0, format-date-time(today(),&quot;%m&quot;) -  /data/new_contact/ephemeral_dob/age_months  + 12, format-date-time(today(),&quot;%m&quot;) -  /data/new_contact/ephemeral_dob/age_months )"></bind>
      <bind nodeset="/data/new_contact/ephemeral_dob/ephemeral_years" type="string" calculate="if(format-date-time(today(),&quot;%m&quot;) -  /data/new_contact/ephemeral_dob/age_months  &lt; 0, format-date-time(today(),&quot;%Y&quot;) -  /data/new_contact/ephemeral_dob/age_years  - 1, format-date-time(today(),&quot;%Y&quot;) - /data/new_contact/ephemeral_dob/age_years )"></bind>
      <bind nodeset="/data/new_contact/ephemeral_dob/dob_approx" type="string" calculate="concat(string( /data/new_contact/ephemeral_dob/ephemeral_years ),'-',if( /data/new_contact/ephemeral_dob/ephemeral_months &lt;10, concat('0',string( /data/new_contact/ephemeral_dob/ephemeral_months )),  /data/new_contact/ephemeral_dob/ephemeral_months ),'-',string(format-date-time(today(), &quot;%d&quot;)))"></bind>
      <bind nodeset="/data/new_contact/ephemeral_dob/dob_raw" type="string" calculate="if(not(selected(  /data/new_contact/ephemeral_dob/dob_method ,'approx')), &#10; /data/new_contact/ephemeral_dob/dob_calendar ,&#10; /data/new_contact/ephemeral_dob/dob_approx )"></bind>
      <bind nodeset="/data/new_contact/ephemeral_dob/dob_iso" type="string" calculate="format-date-time( /data/new_contact/ephemeral_dob/dob_raw ,&quot;%Y-%m-%d&quot;)"></bind>
      <bind nodeset="/data/new_contact/ephemeral_dob/dob_debug" readonly="true()" type="string" relevant="false()"></bind>
      <bind nodeset="/data/new_contact/sex" type="string" required="true()"></bind>
      <bind nodeset="/data/new_contact/phone_read_only" type="string" relevant=" /data/user_phone  != ''" calculate=" /data/user_phone " readonly="true()"></bind>
      <bind nodeset="/data/new_contact/new_phone" type="tel" required="true()" relevant=" /data/user_phone  = ''" constraint="true()" jr:constraintMsg="jr:itext('/data/new_contact/new_phone:jr:constraintMsg')"></bind>
      <bind nodeset="/data/new_contact/phone" type="string" calculate="coalesce( /data/user_phone ,  /data/new_contact/new_phone )"></bind>
      <bind nodeset="/data/new_contact/phone_alternate" type="tel" constraint="true()" jr:constraintMsg="jr:itext('/data/new_contact/phone_alternate:jr:constraintMsg')"></bind>
      <bind nodeset="/data/new_contact/role" type="string" calculate=" /data/contact_role " readonly="true()"></bind>
      <bind nodeset="/data/new_contact/external_id" type="string"></bind>
      <bind nodeset="/data/new_contact/meta/created_by" type="string" calculate="../../../created_by"></bind>
      <bind nodeset="/data/new_contact/meta/created_by_person_uuid" type="string" calculate="../../../created_by_person_uuid"></bind>
      <bind nodeset="/data/new_contact/meta/created_by_place_uuid" type="string" calculate="../../../created_by_place_uuid"></bind>
      <bind nodeset="/data/new_contact/created_by_doc" type="string" calculate="."></bind>
      <bind nodeset="/data/replacement_contact_id" type="string" calculate=" /data/new_contact "></bind>
      <bind nodeset="/data/new_contact_name" type="string" calculate="../new_contact/name"></bind>
      <bind nodeset="/data/new_contact_phone" type="string" calculate="../new_contact/phone"></bind>
      <bind nodeset="/data/outro/warning_header" readonly="true()" type="string"></bind>
      <bind nodeset="/data/outro/warning" readonly="true()" type="string"></bind>
      <bind nodeset="/data/meta/instanceID" type="string" readonly="true()" jr:preload="uid"></bind>
    </model>
