{
  "extends": "@medic",
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "max-len": "off",
    "no-console": "off",
    "no-debugger": "off",
    "quotes": ["error", "single", { "avoidEscape": true }]
  },
  "globals": {
    "contact": true,
    "lineage": true,
    "reports": true
  },
  "overrides": [
    {
      "files": [
        "**/*.json"
      ],
      "plugins": [
        "json"
      ]
    }
  ]
}