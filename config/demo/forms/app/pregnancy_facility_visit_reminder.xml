        <data id="pregnancy_facility_visit_reminder" version="2019-11-04 14:15:19.872000" prefix="J1!pregnancy_facility_visit_reminder!" delimiter="#">
          <inputs>
            <meta>
              <location>
                <lat></lat>
                <long></long>
                <error></error>
                <message></message>
              </location>
            </meta>
            <source>user</source>
            <source_id></source_id>
            <source_visit_date></source_visit_date>
            <contact>
              <_id></_id>
              <name></name>
              <short_name></short_name>
              <patient_id></patient_id>
              <date_of_birth>0</date_of_birth>
              <sex></sex>
              <parent>
                <_id></_id>
                <parent>
                  <contact>
                    <chw_name></chw_name>
                    <phone></phone>
                  </contact>
                </parent>
              </parent>
            </contact>
          </inputs>
          <patient_age_in_years tag="hidden"></patient_age_in_years>
