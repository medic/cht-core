      <instance id="remind_methods">
        <root>
          <item>
            <itextId>remind_methods-0</itextId>
            <name>in_person</name>
          </item>
          <item>
            <itextId>remind_methods-1</itextId>
            <name>by_phone</name>
          </item>
        </root>
      </instance>
      <instance id="translate_woman_label">
        <root>
          <item>
            <itextId>translate_woman_label-0</itextId>
            <name>woman</name>
