      <item><label ref="jr:itext('remind_methods-0')"></label><value>in_person</value></item><item><label ref="jr:itext('remind_methods-1')"></label><value>by_phone</value></item></select1>
      <group appearance="hidden" ref="/data/facility_visit_reminder/custom_translations">
        <label ref="jr:itext('/data/facility_visit_reminder/custom_translations:label')"></label>
        <select1 ref="/data/facility_visit_reminder/custom_translations/custom_woman_label_translator">
          <label ref="jr:itext('/data/facility_visit_reminder/custom_translations/custom_woman_label_translator:label')"></label>
          
        <item><label ref="jr:itext('translate_woman_label-0')"></label><value>woman</value></item></select1>
        <select1 ref="/data/facility_visit_reminder/custom_translations/custom_woman_start_label_translator">
          <label ref="jr:itext('/data/facility_visit_reminder/custom_translations/custom_woman_start_label_translator:label')"></label>
          
        <item><label ref="jr:itext('translate_woman_start_label-0')"></label><value>woman-start</value></item></select1>
      </group>
    </group>
    <group appearance="hidden" ref="/data/data">
      <label ref="jr:itext('/data/data:label')"></label>
      <group ref="/data/data/meta">
        <label ref="jr:itext('/data/data/meta:label')"></label>
      </group>
    </group>
