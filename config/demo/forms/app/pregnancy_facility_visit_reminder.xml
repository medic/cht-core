<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:odk="http://www.opendatakit.org/xforms">
  <h:head>
    <h:title>Health facility ANC reminder</h:title>
    <model odk:xforms-version="1.0.0">
      <itext>
        <translation lang="en">
          <text id="remind_methods-0">
            <value>In person</value>
          </text>
          <text id="remind_methods-1">
            <value>By phone</value>
          </text>
          <text id="translate_woman_label-0">
            <value>the woman</value>
          </text>
          <text id="translate_woman_start_label-0">
            <value>The woman</value>
          </text>
          <text id="/data/inputs/source:label">
            <value>Source</value>
          </text>
          <text id="/data/inputs/source_id:label">
            <value>Source ID</value>
          </text>
          <text id="/data/inputs/source_visit_date:label">
            <value>Visit Date</value>
          </text>
          <text id="/data/inputs/contact/_id:label">
            <value>What is the patient's name?</value>
          </text>
          <text id="/data/inputs/contact/name:label">
            <value>Name</value>
          </text>
          <text id="/data/inputs/contact/short_name:label">
            <value>Short Name</value>
          </text>
          <text id="/data/inputs/contact/patient_id:label">
            <value>Patient ID</value>
          </text>
          <text id="/data/inputs/contact/date_of_birth:label">
            <value>Date of Birth</value>
          </text>
          <text id="/data/inputs/contact/sex:label">
            <value>Sex</value>
          </text>
          <text id="/data/inputs/contact/parent/_id:label">
            <value>Parent ID</value>
          </text>
          <text id="/data/inputs/contact/parent/parent/contact/chw_name:label">
            <value>CHW name</value>
          </text>
          <text id="/data/inputs/contact/parent/parent/contact/phone:label">
            <value>CHW phone</value>
          </text>
          <text id="/data/facility_visit_reminder:label">
            <value>Reminder of Upcoming Health Facility Visit</value>
          </text>
          <text id="/data/facility_visit_reminder/remind_note:label">
            <value> Please remind the client to attend their ANC visit on <output value=" /data/visit_date_for_task "/>.</value>
          </text>
          <text id="/data/facility_visit_reminder/remind_method:label">
            <value>Did you remind the client in-person or by phone?</value>
          </text>
        </translation>
      </itext>
      <instance>
        <data id="pregnancy_facility_visit_reminder" version="2019-11-04 14:15:19.872000" prefix="J1!pregnancy_facility_visit_reminder!" delimiter="#">
          <inputs>
            <meta>
              <location>
                <lat/>
                <long/>
                <error/>
                <message/>
              </location>
            </meta>
            <source>user</source>
            <source_id/>
            <source_visit_date/>
            <contact>
              <_id/>
              <name/>
              <short_name/>
              <patient_id/>
              <date_of_birth>0</date_of_birth>
              <sex/>
              <parent>
                <_id/>
                <parent>
                  <contact>
                    <chw_name/>
                    <phone/>
                  </contact>
                </parent>
              </parent>
            </contact>
          </inputs>
          <patient_age_in_years tag="hidden"/>
          <patient_uuid tag="hidden"/>
          <patient_id tag="hidden"/>
          <patient_name tag="hidden"/>
          <patient_short_name tag="hidden"/>
          <patient_short_name_start tag="hidden"/>
          <pregnancy_uuid_ctx/>
          <visit_date_for_task/>
          <facility_visit_reminder>
            <remind_note tag="hidden"/>
            <remind_method/>
            <custom_translations tag="hidden">
              <custom_woman_label_translator/>
              <custom_woman_label/>
              <custom_woman_start_label_translator/>
              <custom_woman_start_label/>
            </custom_translations>
          </facility_visit_reminder>
          <data tag="hidden">
            <__how_reminded/>
            <meta tag="hidden">
              <__patient_uuid/>
              <__patient_id/>
              <__household_uuid/>
              <__source/>
              <__source_id/>
              <__pregnancy_uuid/>
            </meta>
          </data>
          <meta tag="hidden">
            <instanceID/>
          </meta>
        </data>
      </instance>
      <instance id="contact-summary"/>
      <instance id="user-contact-summary"/>
      <instance id="remind_methods">
        <root>
          <item>
            <itextId>remind_methods-0</itextId>
            <name>in_person</name>
          </item>
          <item>
            <itextId>remind_methods-1</itextId>
            <name>by_phone</name>
          </item>
        </root>
      </instance>
      <instance id="translate_woman_label">
        <root>
          <item>
            <itextId>translate_woman_label-0</itextId>
            <name>woman</name>
          </item>
        </root>
      </instance>
      <instance id="translate_woman_start_label">
        <root>
          <item>
            <itextId>translate_woman_start_label-0</itextId>
            <name>woman-start</name>
          </item>
        </root>
      </instance>
      <bind nodeset="/data/inputs" relevant="./source = 'user'"/>
      <bind nodeset="/data/inputs/source" type="string"/>
      <bind nodeset="/data/inputs/source_id" type="string"/>
      <bind nodeset="/data/inputs/source_visit_date" type="string"/>
      <bind nodeset="/data/inputs/contact/_id" type="db:person"/>
      <bind nodeset="/data/inputs/contact/name" type="string"/>
      <bind nodeset="/data/inputs/contact/short_name" type="string"/>
      <bind nodeset="/data/inputs/contact/patient_id" type="string"/>
      <bind nodeset="/data/inputs/contact/date_of_birth" type="string"/>
      <bind nodeset="/data/inputs/contact/sex" type="string"/>
      <bind nodeset="/data/inputs/contact/parent/_id" type="string"/>
      <bind nodeset="/data/inputs/contact/parent/parent/contact/chw_name" type="string"/>
      <bind nodeset="/data/inputs/contact/parent/parent/contact/phone" type="string"/>
      <bind nodeset="/data/patient_age_in_years" type="string" calculate="floor( difference-in-months( ../inputs/contact/date_of_birth, today() ) div 12 )"/>
      <bind nodeset="/data/patient_uuid" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/data/patient_id" type="string" calculate="../inputs/contact/patient_id"/>
      <bind nodeset="/data/patient_name" type="string" calculate="../inputs/contact/name"/>
      <bind nodeset="/data/patient_short_name" type="string" calculate="coalesce(../inputs/contact/short_name, ../facility_visit_reminder/custom_translations/custom_woman_label)"/>
      <bind nodeset="/data/patient_short_name_start" type="string" calculate="coalesce(../inputs/contact/short_name, ../facility_visit_reminder/custom_translations/custom_woman_start_label)"/>
      <bind nodeset="/data/pregnancy_uuid_ctx" type="string" calculate="if(instance('contact-summary')/context/pregnancy_uuid != '', instance('contact-summary')/context/pregnancy_uuid, .)"/>
      <bind nodeset="/data/visit_date_for_task" type="string" calculate="format-date-time(../inputs/source_visit_date, &quot;%e %b, %Y&quot;)"/>
      <bind nodeset="/data/facility_visit_reminder/remind_note" readonly="true()" type="string"/>
      <bind nodeset="/data/facility_visit_reminder/remind_method" type="string"/>
      <bind nodeset="/data/facility_visit_reminder/custom_translations/custom_woman_label_translator" type="string" calculate="&quot;woman&quot;"/>
      <bind nodeset="/data/facility_visit_reminder/custom_translations/custom_woman_label" type="string" calculate="jr:choice-name( /data/facility_visit_reminder/custom_translations/custom_woman_label_translator ,' /data/facility_visit_reminder/custom_translations/custom_woman_label_translator ')"/>
      <bind nodeset="/data/facility_visit_reminder/custom_translations/custom_woman_start_label_translator" type="string" calculate="&quot;woman-start&quot;"/>
      <bind nodeset="/data/facility_visit_reminder/custom_translations/custom_woman_start_label" type="string" calculate="jr:choice-name( /data/facility_visit_reminder/custom_translations/custom_woman_start_label_translator ,' /data/facility_visit_reminder/custom_translations/custom_woman_start_label_translator ')"/>
      <bind nodeset="/data/data/__how_reminded" type="string" calculate=" /data/facility_visit_reminder/remind_method "/>
      <bind nodeset="/data/data/meta/__patient_uuid" type="string" calculate="../../../inputs/contact/_id"/>
      <bind nodeset="/data/data/meta/__patient_id" type="string" calculate="../../../inputs/contact/patient_id"/>
      <bind nodeset="/data/data/meta/__household_uuid" type="string" calculate="../../../inputs/contact/parent/_id"/>
      <bind nodeset="/data/data/meta/__source" type="string" calculate="../../../inputs/source"/>
      <bind nodeset="/data/data/meta/__source_id" type="string" calculate="../../../inputs/source_id"/>
      <bind nodeset="/data/data/meta/__pregnancy_uuid" type="string" calculate=" /data/pregnancy_uuid_ctx "/>
      <bind nodeset="/data/meta/instanceID" type="string" readonly="true()" jr:preload="uid"/>
    </model>
  </h:head>
  <h:body class="pages">
    <group appearance="field-list" ref="/data/inputs">
      <group ref="/data/inputs/contact">
        <input ref="/data/inputs/contact/_id" appearance="db-object">
          <label ref="jr:itext('/data/inputs/contact/_id:label')"/>
        </input>
        <group ref="/data/inputs/contact/parent">
          <group ref="/data/inputs/contact/parent/parent">
            <group ref="/data/inputs/contact/parent/parent/contact"></group>
          </group>
        </group>
      </group>
    </group>
    <group appearance="field-list" ref="/data/facility_visit_reminder">
      <label ref="jr:itext('/data/facility_visit_reminder:label')"/>
      <input ref="/data/facility_visit_reminder/remind_note">
        <label ref="jr:itext('/data/facility_visit_reminder/remind_note:label')"/>
      </input>
      <select1 ref="/data/facility_visit_reminder/remind_method">
        <label ref="jr:itext('/data/facility_visit_reminder/remind_method:label')"/>
        <itemset nodeset="instance('remind_methods')/root/item">
          <value ref="name"/>
          <label ref="jr:itext(itextId)"/>
        </itemset>
      </select1>
      <group appearance="hidden" ref="/data/facility_visit_reminder/custom_translations">
        <select1 ref="/data/facility_visit_reminder/custom_translations/custom_woman_label_translator">
          <itemset nodeset="instance('translate_woman_label')/root/item">
            <value ref="name"/>
            <label ref="jr:itext(itextId)"/>
          </itemset>
        </select1>
        <select1 ref="/data/facility_visit_reminder/custom_translations/custom_woman_start_label_translator">
          <itemset nodeset="instance('translate_woman_start_label')/root/item">
            <value ref="name"/>
            <label ref="jr:itext(itextId)"/>
          </itemset>
        </select1>
      </group>
    </group>
    <group appearance="hidden" ref="/data/data">
      <group ref="/data/data/meta"></group>
    </group>
  </h:body>
</h:html>