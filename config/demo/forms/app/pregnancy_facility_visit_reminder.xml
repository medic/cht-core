        </root>
      </instance>
      <instance id="translate_woman_start_label">
        <root>
          <item>
            <itextId>translate_woman_start_label-0</itextId>
            <name>woman-start</name>
          </item>
        </root>
      </instance>
      <bind nodeset="/data/inputs" relevant="./source = 'user'"></bind>
      <bind nodeset="/data/inputs/source" type="string"></bind>
      <bind nodeset="/data/inputs/source_id" type="string"></bind>
      <bind nodeset="/data/inputs/source_visit_date" type="string"></bind>
      <bind nodeset="/data/inputs/contact/_id" type="db:person"></bind>
      <bind nodeset="/data/inputs/contact/name" type="string"></bind>
      <bind nodeset="/data/inputs/contact/short_name" type="string"></bind>
      <bind nodeset="/data/inputs/contact/patient_id" type="string"></bind>
      <bind nodeset="/data/inputs/contact/date_of_birth" type="string"></bind>
      <bind nodeset="/data/inputs/contact/sex" type="string"></bind>
      <bind nodeset="/data/inputs/contact/parent/_id" type="string"></bind>
      <bind nodeset="/data/inputs/contact/parent/parent/contact/chw_name" type="string"></bind>
      <bind nodeset="/data/inputs/contact/parent/parent/contact/phone" type="string"></bind>
      <bind nodeset="/data/patient_age_in_years" type="string" calculate="floor( difference-in-months( ../inputs/contact/date_of_birth, today() ) div 12 )"></bind>
      <bind nodeset="/data/patient_uuid" type="string" calculate="../inputs/contact/_id"></bind>
      <bind nodeset="/data/patient_id" type="string" calculate="../inputs/contact/patient_id"></bind>
      <bind nodeset="/data/patient_name" type="string" calculate="../inputs/contact/name"></bind>
      <bind nodeset="/data/patient_short_name" type="string" calculate="coalesce(../inputs/contact/short_name, ../facility_visit_reminder/custom_translations/custom_woman_label)"></bind>
      <bind nodeset="/data/patient_short_name_start" type="string" calculate="coalesce(../inputs/contact/short_name, ../facility_visit_reminder/custom_translations/custom_woman_start_label)"></bind>
      <bind nodeset="/data/pregnancy_uuid_ctx" type="string" calculate="if(instance('contact-summary')/context/pregnancy_uuid != '', instance('contact-summary')/context/pregnancy_uuid, .)"></bind>
      <bind nodeset="/data/visit_date_for_task" type="string" calculate="format-date-time(../inputs/source_visit_date, &quot;%e %b, %Y&quot;)"></bind>
      <bind nodeset="/data/facility_visit_reminder/remind_note" readonly="true()" type="string"></bind>
      <bind nodeset="/data/facility_visit_reminder/remind_method" type="string"></bind>
      <bind nodeset="/data/facility_visit_reminder/custom_translations/custom_woman_label_translator" type="string" calculate="&quot;woman&quot;"></bind>
      <bind nodeset="/data/facility_visit_reminder/custom_translations/custom_woman_label" type="string" calculate="jr:choice-name( /data/facility_visit_reminder/custom_translations/custom_woman_label_translator ,' /data/facility_visit_reminder/custom_translations/custom_woman_label_translator ')"></bind>
      <bind nodeset="/data/facility_visit_reminder/custom_translations/custom_woman_start_label_translator" type="string" calculate="&quot;woman-start&quot;"></bind>
      <bind nodeset="/data/facility_visit_reminder/custom_translations/custom_woman_start_label" type="string" calculate="jr:choice-name( /data/facility_visit_reminder/custom_translations/custom_woman_start_label_translator ,' /data/facility_visit_reminder/custom_translations/custom_woman_start_label_translator ')"></bind>
      <bind nodeset="/data/data/__how_reminded" type="string" calculate=" /data/facility_visit_reminder/remind_method "></bind>
      <bind nodeset="/data/data/meta/__patient_uuid" type="string" calculate="../../../inputs/contact/_id"></bind>
      <bind nodeset="/data/data/meta/__patient_id" type="string" calculate="../../../inputs/contact/patient_id"></bind>
      <bind nodeset="/data/data/meta/__household_uuid" type="string" calculate="../../../inputs/contact/parent/_id"></bind>
      <bind nodeset="/data/data/meta/__source" type="string" calculate="../../../inputs/source"></bind>
      <bind nodeset="/data/data/meta/__source_id" type="string" calculate="../../../inputs/source_id"></bind>
      <bind nodeset="/data/data/meta/__pregnancy_uuid" type="string" calculate=" /data/pregnancy_uuid_ctx "></bind>
      <bind nodeset="/data/meta/instanceID" type="string" readonly="true()" jr:preload="uid"></bind>
    </model>
  </h:head>
