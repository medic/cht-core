<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:odk="http://www.opendatakit.org/xforms">
  <h:head>
    <h:title>PNC danger sign follow-up - baby</h:title>
    <model odk:xforms-version="1.0.0">
      <itext>
        <translation lang="en">
          <text id="yes_no-0">
            <value>Yes</value>
          </text>
          <text id="yes_no-1">
            <value>No</value>
          </text>
          <text id="/data/inputs/source:label">
            <value>Source</value>
          </text>
          <text id="/data/inputs/source_id:label">
            <value>Source ID</value>
          </text>
          <text id="/data/inputs/delivery_uuid:label">
            <value>Delivery Report ID</value>
          </text>
          <text id="/data/inputs/contact/_id:label">
            <value>What is the patient's name?</value>
          </text>
          <text id="/data/inputs/contact/name:label">
            <value>Name</value>
          </text>
          <text id="/data/inputs/contact/short_name:label">
            <value>Short Name</value>
          </text>
          <text id="/data/inputs/contact/patient_id:label">
            <value>Patient ID</value>
          </text>
          <text id="/data/inputs/contact/date_of_birth:label">
            <value>Date of Birth</value>
          </text>
          <text id="/data/inputs/contact/sex:label">
            <value>Sex</value>
          </text>
          <text id="/data/inputs/contact/parent/_id:label">
            <value>Parent ID</value>
          </text>
          <text id="/data/inputs/contact/parent/parent/contact/chw_name:label">
            <value>CHW name</value>
          </text>
          <text id="/data/inputs/contact/parent/parent/contact/phone:label">
            <value>CHW phone</value>
          </text>
          <text id="/data/danger_signs:label">
            <value>Danger Sign Follow-up - Baby</value>
          </text>
          <text id="/data/danger_signs/visit_confirm:label">
            <value>Was the baby taken to the health facility as recommended?</value>
          </text>
          <text id="/data/danger_signs/danger_sign_present:label">
            <value>Is the baby still experiencing any danger signs?</value>
          </text>
          <text id="/data/danger_signs/danger_signs_question_note:label">
            <value>Please confirm if the baby has any of the following danger signs.</value>
          </text>
          <text id="/data/danger_signs/infected_umbilical_cord:label">
            <value>Infected umbilical cord</value>
          </text>
          <text id="/data/danger_signs/convulsion:label">
            <value>Convulsions</value>
          </text>
          <text id="/data/danger_signs/difficulty_feeding:label">
            <value>Difficulty feeding or drinking</value>
          </text>
          <text id="/data/danger_signs/vomit:label">
            <value>Vomits everything</value>
          </text>
          <text id="/data/danger_signs/drowsy:label">
            <value>Drowsy or unconscious</value>
          </text>
          <text id="/data/danger_signs/stiff:label">
            <value>Body stiffness</value>
          </text>
          <text id="/data/danger_signs/yellow_skin:label">
            <value>Yellow skin color</value>
          </text>
          <text id="/data/danger_signs/fever:label">
            <value>Fever</value>
          </text>
          <text id="/data/danger_signs/blue_skin:label">
            <value>Blue skin color (hypothermia)</value>
          </text>
          <text id="/data/danger_signs/congratulate_no_ds_note:label">
            <value>Great news! Please continue to closely monitor the baby.</value>
          </text>
          <text id="/data/danger_signs/refer_patient_note_1:label">
            <value>The baby should be taken to the health facility immediately if experiencing any of these danger signs.</value>
          </text>
          <text id="/data/danger_signs/refer_patient_note_2:label">
            <value>Please advise the caregiver to do so and accompany them if possible.</value>
          </text>
        </translation>
      </itext>
      <instance>
        <data id="pnc_danger_sign_follow_up_baby" version="2022-09-26 09:13:25" prefix="J1!pnc_danger_sign_follow_up_baby!" delimiter="#">
          <inputs>
            <meta>
              <location>
                <lat/>
                <long/>
                <error/>
                <message/>
              </location>
            </meta>
            <source>user</source>
            <source_id/>
            <delivery_uuid/>
            <contact>
              <_id/>
              <name/>
              <short_name/>
              <patient_id/>
              <date_of_birth>0</date_of_birth>
              <sex/>
              <parent>
                <_id/>
                <parent>
                  <contact>
                    <chw_name/>
                    <phone/>
                  </contact>
                </parent>
              </parent>
            </contact>
          </inputs>
          <patient_age_in_years tag="hidden"/>
          <patient_uuid tag="hidden"/>
          <patient_id tag="hidden"/>
          <patient_name tag="hidden"/>
          <t_danger_signs_referral_follow_up_date tag="hidden"/>
          <t_danger_signs_referral_follow_up tag="hidden"/>
          <danger_signs>
            <visit_confirm/>
            <danger_sign_present/>
            <danger_signs_question_note tag="hidden"/>
            <infected_umbilical_cord/>
            <convulsion/>
            <difficulty_feeding/>
            <vomit/>
            <drowsy/>
            <stiff/>
            <yellow_skin/>
            <fever/>
            <blue_skin/>
            <r_danger_sign_present tag="hidden"/>
            <congratulate_no_ds_note tag="hidden"/>
            <refer_patient_note_1 tag="hidden"/>
            <refer_patient_note_2 tag="hidden"/>
          </danger_signs>
          <data tag="hidden">
            <__visited_hf/>
            <__still_experiencing_danger_sign/>
            <__infected_umbilical_cord/>
            <__convulsions/>
            <__difficulty_feeding_or_drinking/>
            <__vomits_everything/>
            <__drowsy_or_unconscious/>
            <__body_stiffness/>
            <__yellow_skin_color/>
            <__fever/>
            <__blue_skin_color/>
            <__has_danger_sign/>
            <meta tag="hidden">
              <__patient_uuid/>
              <__patient_id/>
              <__household_uuid/>
              <__source/>
              <__source_id/>
              <__delivery_uuid/>
            </meta>
          </data>
          <meta tag="hidden">
            <instanceID/>
          </meta>
        </data>
      </instance>
      <instance id="contact-summary"/>
      <instance id="user-contact-summary"/>
      <instance id="yes_no">
        <root>
          <item>
            <itextId>yes_no-0</itextId>
            <name>yes</name>
          </item>
          <item>
            <itextId>yes_no-1</itextId>
            <name>no</name>
          </item>
        </root>
      </instance>
      <bind nodeset="/data/inputs" relevant="./source = 'user'"/>
      <bind nodeset="/data/inputs/source" type="string"/>
      <bind nodeset="/data/inputs/source_id" type="string"/>
      <bind nodeset="/data/inputs/delivery_uuid" type="string"/>
      <bind nodeset="/data/inputs/contact/_id" type="db:person"/>
      <bind nodeset="/data/inputs/contact/name" type="string"/>
      <bind nodeset="/data/inputs/contact/short_name" type="string"/>
      <bind nodeset="/data/inputs/contact/patient_id" type="string"/>
      <bind nodeset="/data/inputs/contact/date_of_birth" type="string"/>
      <bind nodeset="/data/inputs/contact/sex" type="string"/>
      <bind nodeset="/data/inputs/contact/parent/_id" type="string"/>
      <bind nodeset="/data/inputs/contact/parent/parent/contact/chw_name" type="string"/>
      <bind nodeset="/data/inputs/contact/parent/parent/contact/phone" type="string"/>
      <bind nodeset="/data/patient_age_in_years" type="string" calculate="floor( difference-in-months( ../inputs/contact/date_of_birth, today() ) div 12 )"/>
      <bind nodeset="/data/patient_uuid" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/data/patient_id" type="string" calculate="../inputs/contact/patient_id"/>
      <bind nodeset="/data/patient_name" type="string" calculate="../inputs/contact/name"/>
      <bind nodeset="/data/t_danger_signs_referral_follow_up_date" type="string" calculate="date-time(decimal-date-time(today()) + 3)"/>
      <bind nodeset="/data/t_danger_signs_referral_follow_up" type="string" calculate="../danger_signs/r_danger_sign_present"/>
      <bind nodeset="/data/danger_signs/visit_confirm" type="string" required="true()"/>
      <bind nodeset="/data/danger_signs/danger_sign_present" type="string" required="true()"/>
      <bind nodeset="/data/danger_signs/danger_signs_question_note" readonly="true()" type="string" relevant="../danger_sign_present = 'yes' or ../danger_sign_present = 'no'"/>
      <bind nodeset="/data/danger_signs/infected_umbilical_cord" type="string" required="true()" relevant="../danger_sign_present = 'yes' or ../danger_sign_present = 'no'"/>
      <bind nodeset="/data/danger_signs/convulsion" type="string" required="true()" relevant="../danger_sign_present = 'yes' or ../danger_sign_present = 'no'"/>
      <bind nodeset="/data/danger_signs/difficulty_feeding" type="string" required="true()" relevant="../danger_sign_present = 'yes' or ../danger_sign_present = 'no'"/>
      <bind nodeset="/data/danger_signs/vomit" type="string" required="true()" relevant="../danger_sign_present = 'yes' or ../danger_sign_present = 'no'"/>
      <bind nodeset="/data/danger_signs/drowsy" type="string" required="true()" relevant="../danger_sign_present = 'yes' or ../danger_sign_present = 'no'"/>
      <bind nodeset="/data/danger_signs/stiff" type="string" required="true()" relevant="../danger_sign_present = 'yes' or ../danger_sign_present = 'no'"/>
      <bind nodeset="/data/danger_signs/yellow_skin" type="string" required="true()" relevant="../danger_sign_present = 'yes' or ../danger_sign_present = 'no'"/>
      <bind nodeset="/data/danger_signs/fever" type="string" required="true()" relevant="../danger_sign_present = 'yes' or ../danger_sign_present = 'no'"/>
      <bind nodeset="/data/danger_signs/blue_skin" type="string" required="true()" relevant="../danger_sign_present = 'yes' or ../danger_sign_present = 'no'"/>
      <bind nodeset="/data/danger_signs/r_danger_sign_present" type="string" calculate="if(selected(../infected_umbilical_cord, 'yes') or&#10;selected(../convulsion, 'yes') or&#10;selected(../difficulty_feeding, 'yes') or&#10;selected(../vomit, 'yes') or&#10;selected(../drowsy, 'yes') or&#10;selected(../stiff, 'yes') or&#10;selected(../yellow_skin, 'yes') or&#10;selected(../fever, 'yes') or&#10;selected(../blue_skin, 'yes'), 'yes', &#10;if(selected(../infected_umbilical_cord, 'no') and&#10;selected(../convulsion, 'no') and&#10;selected(../difficulty_feeding, 'no') and&#10;selected(../vomit, 'no') and&#10;selected(../drowsy, 'no') and&#10;selected(../stiff, 'no') and&#10;selected(../yellow_skin, 'no') and&#10;selected(../fever, 'no') and&#10;selected(../blue_skin, 'no'), 'no', ''))"/>
      <bind nodeset="/data/danger_signs/congratulate_no_ds_note" readonly="true()" type="string" relevant="../danger_sign_present = 'no' and ../r_danger_sign_present = 'no'"/>
      <bind nodeset="/data/danger_signs/refer_patient_note_1" readonly="true()" type="string" relevant="../danger_sign_present = 'yes' or ../r_danger_sign_present = 'yes'"/>
      <bind nodeset="/data/danger_signs/refer_patient_note_2" readonly="true()" type="string" relevant="../danger_sign_present = 'yes' or ../r_danger_sign_present = 'yes'"/>
      <bind nodeset="/data/data/__visited_hf" type="string" calculate=" /data/danger_signs/visit_confirm "/>
      <bind nodeset="/data/data/__still_experiencing_danger_sign" type="string" calculate=" /data/danger_signs/danger_sign_present "/>
      <bind nodeset="/data/data/__infected_umbilical_cord" type="string" calculate=" /data/danger_signs/infected_umbilical_cord "/>
      <bind nodeset="/data/data/__convulsions" type="string" calculate=" /data/danger_signs/convulsion "/>
      <bind nodeset="/data/data/__difficulty_feeding_or_drinking" type="string" calculate=" /data/danger_signs/difficulty_feeding "/>
      <bind nodeset="/data/data/__vomits_everything" type="string" calculate=" /data/danger_signs/vomit "/>
      <bind nodeset="/data/data/__drowsy_or_unconscious" type="string" calculate=" /data/danger_signs/drowsy "/>
      <bind nodeset="/data/data/__body_stiffness" type="string" calculate=" /data/danger_signs/stiff "/>
      <bind nodeset="/data/data/__yellow_skin_color" type="string" calculate=" /data/danger_signs/yellow_skin "/>
      <bind nodeset="/data/data/__fever" type="string" calculate=" /data/danger_signs/fever "/>
      <bind nodeset="/data/data/__blue_skin_color" type="string" calculate=" /data/danger_signs/blue_skin "/>
      <bind nodeset="/data/data/__has_danger_sign" type="string" calculate=" /data/danger_signs/r_danger_sign_present "/>
      <bind nodeset="/data/data/meta/__patient_uuid" type="string" calculate="../../../inputs/contact/_id"/>
      <bind nodeset="/data/data/meta/__patient_id" type="string" calculate="../../../inputs/contact/patient_id"/>
      <bind nodeset="/data/data/meta/__household_uuid" type="string" calculate="../../../inputs/contact/parent/_id"/>
      <bind nodeset="/data/data/meta/__source" type="string" calculate="../../../inputs/source"/>
      <bind nodeset="/data/data/meta/__source_id" type="string" calculate="../../../inputs/source_id"/>
      <bind nodeset="/data/data/meta/__delivery_uuid" type="string" calculate="../../../inputs/delivery_uuid"/>
      <bind nodeset="/data/meta/instanceID" type="string" readonly="true()" jr:preload="uid"/>
    </model>
  </h:head>
  <h:body class="pages">
    <group appearance="field-list" ref="/data/inputs">
      <group ref="/data/inputs/contact">
        <input ref="/data/inputs/contact/_id" appearance="db-object">
          <label ref="jr:itext('/data/inputs/contact/_id:label')"/>
        </input>
        <group ref="/data/inputs/contact/parent">
          <group ref="/data/inputs/contact/parent/parent">
            <group ref="/data/inputs/contact/parent/parent/contact"/>
          </group>
        </group>
      </group>
    </group>
    <group appearance="field-list" ref="/data/danger_signs">
      <label ref="jr:itext('/data/danger_signs:label')"/>
      <select1 ref="/data/danger_signs/visit_confirm">
        <label ref="jr:itext('/data/danger_signs/visit_confirm:label')"/>
        <itemset nodeset="instance('yes_no')/root/item">
          <value ref="name"/>
          <label ref="jr:itext(itextId)"/>
        </itemset>
      </select1>
      <select1 ref="/data/danger_signs/danger_sign_present">
        <label ref="jr:itext('/data/danger_signs/danger_sign_present:label')"/>
        <itemset nodeset="instance('yes_no')/root/item">
          <value ref="name"/>
          <label ref="jr:itext(itextId)"/>
        </itemset>
      </select1>
      <input ref="/data/danger_signs/danger_signs_question_note">
        <label ref="jr:itext('/data/danger_signs/danger_signs_question_note:label')"/>
      </input>
      <select1 ref="/data/danger_signs/infected_umbilical_cord">
        <label ref="jr:itext('/data/danger_signs/infected_umbilical_cord:label')"/>
        <itemset nodeset="instance('yes_no')/root/item">
          <value ref="name"/>
          <label ref="jr:itext(itextId)"/>
        </itemset>
      </select1>
      <select1 ref="/data/danger_signs/convulsion">
        <label ref="jr:itext('/data/danger_signs/convulsion:label')"/>
        <itemset nodeset="instance('yes_no')/root/item">
          <value ref="name"/>
          <label ref="jr:itext(itextId)"/>
        </itemset>
      </select1>
      <select1 ref="/data/danger_signs/difficulty_feeding">
        <label ref="jr:itext('/data/danger_signs/difficulty_feeding:label')"/>
        <itemset nodeset="instance('yes_no')/root/item">
          <value ref="name"/>
          <label ref="jr:itext(itextId)"/>
        </itemset>
      </select1>
      <select1 ref="/data/danger_signs/vomit">
        <label ref="jr:itext('/data/danger_signs/vomit:label')"/>
        <itemset nodeset="instance('yes_no')/root/item">
          <value ref="name"/>
          <label ref="jr:itext(itextId)"/>
        </itemset>
      </select1>
      <select1 ref="/data/danger_signs/drowsy">
        <label ref="jr:itext('/data/danger_signs/drowsy:label')"/>
        <itemset nodeset="instance('yes_no')/root/item">
          <value ref="name"/>
          <label ref="jr:itext(itextId)"/>
        </itemset>
      </select1>
      <select1 ref="/data/danger_signs/stiff">
        <label ref="jr:itext('/data/danger_signs/stiff:label')"/>
        <itemset nodeset="instance('yes_no')/root/item">
          <value ref="name"/>
          <label ref="jr:itext(itextId)"/>
        </itemset>
      </select1>
      <select1 ref="/data/danger_signs/yellow_skin">
        <label ref="jr:itext('/data/danger_signs/yellow_skin:label')"/>
        <itemset nodeset="instance('yes_no')/root/item">
          <value ref="name"/>
          <label ref="jr:itext(itextId)"/>
        </itemset>
      </select1>
      <select1 ref="/data/danger_signs/fever">
        <label ref="jr:itext('/data/danger_signs/fever:label')"/>
        <itemset nodeset="instance('yes_no')/root/item">
          <value ref="name"/>
          <label ref="jr:itext(itextId)"/>
        </itemset>
      </select1>
      <select1 ref="/data/danger_signs/blue_skin">
        <label ref="jr:itext('/data/danger_signs/blue_skin:label')"/>
        <itemset nodeset="instance('yes_no')/root/item">
          <value ref="name"/>
          <label ref="jr:itext(itextId)"/>
        </itemset>
      </select1>
      <input ref="/data/danger_signs/congratulate_no_ds_note">
        <label ref="jr:itext('/data/danger_signs/congratulate_no_ds_note:label')"/>
      </input>
      <input ref="/data/danger_signs/refer_patient_note_1">
        <label ref="jr:itext('/data/danger_signs/refer_patient_note_1:label')"/>
      </input>
      <input ref="/data/danger_signs/refer_patient_note_2">
        <label ref="jr:itext('/data/danger_signs/refer_patient_note_2:label')"/>
      </input>
    </group>
    <group appearance="hidden" ref="/data/data">
      <group ref="/data/data/meta"/>
    </group>
  </h:body>
</h:html>