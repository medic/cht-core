      <bind nodeset="/data/person/parent" type="string"/>
      <bind nodeset="/data/person/type" type="string"/>
      <bind nodeset="/data/person/first_name" type="string" required="true()"/>
      <bind nodeset="/data/person/last_name" type="string" required="true()"/>
      <bind nodeset="/data/person/name" type="string" calculate="concat( /data/person/first_name , ' ',  /data/person/last_name )"/>
