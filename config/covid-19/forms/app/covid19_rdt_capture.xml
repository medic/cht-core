      <bind nodeset="/data/summary/time_read" type="string" calculate="format-date-time(parse-timestamp-to-date( /data/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_result_bundle/rdt_session_result_time_read ), &quot;%e %b, %Y at %H:%M:%S&quot;)"></bind>
      <bind nodeset="/data/summary/time_expired" type="string" calculate="format-date-time(parse-timestamp-to-date( /data/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_time_expired ), &quot;%e %b, %Y at %H:%M:%S&quot;)"></bind>
      <bind nodeset="/data/summary/results_expired" type="string" calculate="if( /data/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_result_bundle/rdt_session_result_time_read  &gt;  /data/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_time_expired , 'TRUE', 'FALSE')"></bind>
      <bind nodeset="/data/summary/result" type="string" calculate="if( /data/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_result_bundle/rdt_session_result_map/sars_cov2 ='sars_cov2_pos', 'Positive', if( /data/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_result_bundle/rdt_session_result_map/sars_cov2 ='sars_cov2_neg', 'Negative', 'Invalid result'))"></bind>
      <bind nodeset="/data/summary/summary_header" readonly="true()" type="string"></bind>
      <bind nodeset="/data/summary/summary_details" readonly="true()" type="string"></bind>
      <bind nodeset="/data/summary/summary_name" readonly="true()" type="string"></bind>
      <bind nodeset="/data/summary/summary_info" readonly="true()" type="string"></bind>
      <bind nodeset="/data/summary/summary_rdt_loinc_long_common_name" readonly="true()" type="string"></bind>
      <bind nodeset="/data/summary/summary_results_expired" readonly="true()" type="string" relevant=" /data/summary/results_expired  = 'TRUE'"></bind>
      <bind nodeset="/data/summary/summary_results_expired_2" readonly="true()" type="string" relevant=" /data/summary/results_expired  = 'TRUE'"></bind>
      <bind nodeset="/data/summary/summary_results_repeat" readonly="true()" type="string" relevant="selected( /data/repeat-test/repeat_test  , 'yes')"></bind>
      <bind nodeset="/data/summary/summary_results" readonly="true()" type="string"></bind>
      <bind nodeset="/data/summary/summary_image" type="string" calculate=" /data/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_result_bundle/rdt_session_result_extra_images/cropped "></bind>
      <bind nodeset="/data/meta/instanceID" type="string" readonly="true()" jr:preload="uid"></bind>
    </model>
  </h:head>
  <h:body class="pages">
    <group ref="/data/inputs">
      <label ref="jr:itext('/data/inputs:label')"></label>
      <group appearance="field-list" ref="/data/inputs/contact">
        <label ref="jr:itext('/data/inputs/contact:label')"></label>
        <input ref="/data/inputs/contact/_id" appearance="select-contact type-person">
          <label ref="jr:itext('/data/inputs/contact/_id:label')"></label>
        </input>
        <group ref="/data/inputs/contact/parent">
          <label ref="jr:itext('/data/inputs/contact/parent:label')"></label>
          <group ref="/data/inputs/contact/parent/parent">
            <label ref="jr:itext('/data/inputs/contact/parent/parent:label')"></label>
          </group>
        </group>
      </group>
    </group>
    <group ref="/data/test-information">
      <label ref="jr:itext('/data/test-information:label')"></label>
    </group>
    <group appearance="field-list" ref="/data/capture">
      <label ref="jr:itext('/data/capture:label')"></label>
      <group appearance="android-app-launcher" ref="/data/capture/rdtoolkit-capture">
        <label ref="jr:itext('/data/capture/rdtoolkit-capture:label')"></label>
        <input ref="/data/capture/rdtoolkit-capture/action" appearance="hidden">
          <label ref="jr:itext('/data/capture/rdtoolkit-capture/action:label')"></label>
        </input>
        <input ref="/data/capture/rdtoolkit-capture/note_instructions_provision">
          <label ref="jr:itext('/data/capture/rdtoolkit-capture/note_instructions_provision:label')"></label>
        </input>
        <group appearance="android-app-inputs" ref="/data/capture/rdtoolkit-capture/android-app-inputs">
          <label ref="jr:itext('/data/capture/rdtoolkit-capture/android-app-inputs:label')"></label>
          <input ref="/data/capture/rdtoolkit-capture/android-app-inputs/rdt_session_id" appearance="hidden">
            <label ref="jr:itext('/data/capture/rdtoolkit-capture/android-app-inputs/rdt_session_id:label')"></label>
          </input>
        </group>
        <group appearance="android-app-outputs" ref="/data/capture/rdtoolkit-capture/android-app-outputs">
          <label ref="jr:itext('/data/capture/rdtoolkit-capture/android-app-outputs:label')"></label>
          <group appearance="android-app-object" ref="/data/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle">
            <label ref="jr:itext('/data/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle:label')"></label>
            <input ref="/data/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_state" appearance="hidden">
              <label ref="jr:itext('/data/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_state:label')"></label>
            </input>
            <input ref="/data/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_time_started" appearance="hidden">
              <label ref="jr:itext('/data/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_time_started:label')"></label>
            </input>
            <input ref="/data/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_time_resolved" appearance="hidden">
              <label ref="jr:itext('/data/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_time_resolved:label')"></label>
            </input>
            <input ref="/data/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_time_expired" appearance="hidden">
              <label ref="jr:itext('/data/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_time_expired:label')"></label>
            </input>
            <input ref="/data/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_test_profile" appearance="hidden">
              <label ref="jr:itext('/data/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_test_profile:label')"></label>
            </input>
            <group appearance="android-app-object" ref="/data/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_result_bundle">
              <label ref="jr:itext('/data/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_result_bundle:label')"></label>
              <input ref="/data/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_result_bundle/rdt_session_result_time_read" appearance="hidden">
                <label ref="jr:itext('/data/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_result_bundle/rdt_session_result_time_read:label')"></label>
              </input>
              <group appearance="android-app-object" ref="/data/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_result_bundle/rdt_session_result_extra_images">
                <label ref="jr:itext('/data/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_result_bundle/rdt_session_result_extra_images:label')"></label>
                <input ref="/data/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_result_bundle/rdt_session_result_extra_images/cropped" appearance="hidden">
                  <label ref="jr:itext('/data/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_result_bundle/rdt_session_result_extra_images/cropped:label')"></label>
                </input>
              </group>
              <group appearance="android-app-object" ref="/data/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_result_bundle/rdt_session_result_map">
                <label ref="jr:itext('/data/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_result_bundle/rdt_session_result_map:label')"></label>
                <input ref="/data/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_result_bundle/rdt_session_result_map/sars_cov2" appearance="hidden">
                  <label ref="jr:itext('/data/capture/rdtoolkit-capture/android-app-outputs/rdt_session_bundle/rdt_session_result_bundle/rdt_session_result_map/sars_cov2:label')"></label>
                </input>
              </group>
            </group>
          </group>
        </group>
        <input ref="/data/capture/rdtoolkit-capture/preview_session_state">
          <label ref="jr:itext('/data/capture/rdtoolkit-capture/preview_session_state:label')"></label>
          <hint ref="jr:itext('/data/capture/rdtoolkit-capture/preview_session_state:hint')"></hint>
        </input>
      </group>
      <input ref="/data/capture/note_continue_1">
        <label ref="jr:itext('/data/capture/note_continue_1:label')"></label>
      </input>
      <input ref="/data/capture/note_continue_2">
        <label ref="jr:itext('/data/capture/note_continue_2:label')"></label>
      </input>
    </group>
    <group appearance="field-list" ref="/data/repeat-test">
      <label ref="jr:itext('/data/repeat-test:label')"></label>
      <select1 ref="/data/repeat-test/repeat_test">
        <label ref="jr:itext('/data/repeat-test/repeat_test:label')"></label>
        
      <item><label ref="jr:itext('yes_no-0')"></label><value>yes</value></item><item><label ref="jr:itext('yes_no-1')"></label><value>no</value></item></select1>
      <select1 ref="/data/repeat-test/repeat_test_reason">
        <label ref="jr:itext('/data/repeat-test/repeat_test_reason:label')"></label>
        
      <item><label ref="jr:itext('repeat_test_reason-0')"></label><value>invalid</value></item><item><label ref="jr:itext('repeat_test_reason-1')"></label><value>indeterminate</value></item><item><label ref="jr:itext('repeat_test_reason-2')"></label><value>lost</value></item><item><label ref="jr:itext('repeat_test_reason-3')"></label><value>other</value></item></select1>
      <input ref="/data/repeat-test/repeat_test_reason_other">
        <label ref="jr:itext('/data/repeat-test/repeat_test_reason_other:label')"></label>
      </input>
    </group>
    <group appearance="field-list summary" ref="/data/summary">
      <label ref="jr:itext('/data/summary:label')"></label>
      <input ref="/data/summary/summary_header" appearance="h2">
        <label ref="jr:itext('/data/summary/summary_header:label')"></label>
      </input>
      <input ref="/data/summary/summary_details" appearance="h1 yellow">
        <label ref="jr:itext('/data/summary/summary_details:label')"></label>
      </input>
      <input ref="/data/summary/summary_name" appearance="center">
        <label ref="jr:itext('/data/summary/summary_name:label')"></label>
      </input>
      <input ref="/data/summary/summary_info" appearance="h1 blue">
        <label ref="jr:itext('/data/summary/summary_info:label')"></label>
      </input>
      <input ref="/data/summary/summary_rdt_loinc_long_common_name" appearance="center">
        <label ref="jr:itext('/data/summary/summary_rdt_loinc_long_common_name:label')"></label>
      </input>
      <input ref="/data/summary/summary_results_expired" appearance="h1 red">
        <label ref="jr:itext('/data/summary/summary_results_expired:label')"></label>
      </input>
      <input ref="/data/summary/summary_results_expired_2" appearance="center">
        <label ref="jr:itext('/data/summary/summary_results_expired_2:label')"></label>
      </input>
      <input ref="/data/summary/summary_results_repeat" appearance="center">
        <label ref="jr:itext('/data/summary/summary_results_repeat:label')"></label>
      </input>
      <input ref="/data/summary/summary_results" appearance="center">
        <label ref="jr:itext('/data/summary/summary_results:label')"></label>
      </input>
      <input ref="/data/summary/summary_image" appearance="display-base64-image">
        <label ref="jr:itext('/data/summary/summary_image:label')"></label>
      </input>
    </group>
  </h:body>
</h:html>