                  <rdt_session_time_expired/>
                  <rdt_session_test_profile/>
                </rdt_session_bundle>
              </android-app-outputs>
              <preview_session_state/>
            </rdtoolkit-provision>
            <note_continue_1/>
            <note_continue_2/>
          </provision>
          <rdt_session_other>
            <rdt_session_other_type/>
          </rdt_session_other>
