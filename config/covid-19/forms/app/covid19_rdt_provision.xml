      <instance>
        <covid19_rdt_provision id="covid19_rdt_provision" prefix="J1!covid19_rdt_provision!" delimiter="#" version="2021-10-22_06-58">
          <inputs>
            <meta>
              <location>
                <lat/>
                <long/>
                <error/>
                <message/>
              </location>
            </meta>
            <user>
              <contact_id/>
              <facility_id/>
              <name/>
            </user>
            <source>user</source>
            <contact>
              <_id/>
              <patient_id/>
              <first_name/>
              <last_name/>
              <name/>
              <date_of_birth/>
              <sex/>
              <phone/>
              <phone_alternate/>
              <address/>
              <parent>
                <_id/>
                <name/>
                <address/>
                <parent>
                  <_id/>
                  <name/>
                  <address/>
                </parent>
              </parent>
            </contact>
          </inputs>
          <patient_uuid/>
          <patient_name/>
          <patient_age_in_years tag="hidden"/>
          <patient_age_in_months tag="hidden"/>
          <patient_age_in_days tag="hidden"/>
          <age/>
          <preview_time_expired>0</preview_time_expired>
          <test-reference>
            <session_id/>
            <test_id/>
            <facility_id/>
            <facility_name/>
            <facility_address/>
            <facility_confirm_ask>yes</facility_confirm_ask>
            <other_facility_name/>
            <other_facility_address/>
            <other_facility_test_setting/>
            <other_test_setting/>
            <test_reason/>
            <symptoms/>
            <days_since_symptoms_began/>
          </test-reference>
          <spec-lot>
            <specimen_type/>
            <specimen_type_other/>
            <rdt_lot/>
            <rdt_lot_expiry_date/>
            <rdt_lot_expiry_date_warning/>
            <additional_notes/>
          </spec-lot>
          <provision tag="hidden">
            <rdtoolkit-provision>
              <action>org.rdtoolkit.action.Provision</action>
              <note_instructions_provision/>
              <android-app-inputs>
                <rdt_session_id/>
                <rdt_config_bundle>
                  <rdt_config_classifier_mode>PRE_POPULATE</rdt_config_classifier_mode>
                  <rdt_config_session_type>TWO_PHASE</rdt_config_session_type>
                  <rdt_config_provision_mode>CRITERIA_SET_AND</rdt_config_provision_mode>
                  <rdt_config_provision_mode_data>sars_cov2</rdt_config_provision_mode_data>
                  <rdt_config_flavor_one/>
                  <rdt_config_flavor_two/>
                  <rdt_config_cloudworks_dns/>
                </rdt_config_bundle>
              </android-app-inputs>
              <android-app-outputs>
                <rdt_session_bundle>
                  <rdt_session_state/>
                  <rdt_session_time_started/>
                  <rdt_session_time_resolved/>
                  <rdt_session_time_expired/>
                  <rdt_session_test_profile/>
                </rdt_session_bundle>
              </android-app-outputs>
              <preview_session_state/>
            </rdtoolkit-provision>
            <note_continue_1/>
            <note_continue_2/>
          </provision>
          <rdt_session_other>
            <rdt_session_other_type/>
          </rdt_session_other>
          <summary tag="hidden">
            <syndrome_test_type>COVID-19</syndrome_test_type>
            <rdt_loinc_code>94558-4</rdt_loinc_code>
            <rdt_loinc_long_common_name>SARS-CoV-2 (COVID-19) Ag [Presence] in Respiratory specimen by Rapid immunoassay</rdt_loinc_long_common_name>
            <summary_header/>
            <summary_details/>
            <summary_name/>
            <summary_info/>
            <summary_test_name/>
            <summary_test_expire/>
          </summary>
          <meta tag="hidden">
            <instanceID/>
          </meta>
        </covid19_rdt_provision>
      </instance>
      <instance id="contact-summary"/>
      <bind nodeset="/covid19_rdt_provision/inputs" relevant="./source = 'user'"/>
      <bind nodeset="/covid19_rdt_provision/inputs/user/contact_id" type="string"/>
      <bind nodeset="/covid19_rdt_provision/inputs/user/facility_id" type="string"/>
      <bind nodeset="/covid19_rdt_provision/inputs/user/name" type="string"/>
      <bind nodeset="/covid19_rdt_provision/inputs/source" type="string"/>
      <bind nodeset="/covid19_rdt_provision/inputs/contact/_id" type="db:person"/>
      <bind nodeset="/covid19_rdt_provision/inputs/contact/patient_id" type="string"/>
      <bind nodeset="/covid19_rdt_provision/inputs/contact/first_name" type="string"/>
      <bind nodeset="/covid19_rdt_provision/inputs/contact/last_name" type="string"/>
      <bind nodeset="/covid19_rdt_provision/inputs/contact/name" type="string"/>
      <bind nodeset="/covid19_rdt_provision/inputs/contact/date_of_birth" type="string"/>
      <bind nodeset="/covid19_rdt_provision/inputs/contact/sex" type="string"/>
      <bind nodeset="/covid19_rdt_provision/inputs/contact/phone" type="string"/>
      <bind nodeset="/covid19_rdt_provision/inputs/contact/phone_alternate" type="string"/>
      <bind nodeset="/covid19_rdt_provision/inputs/contact/address" type="string"/>
      <bind nodeset="/covid19_rdt_provision/inputs/contact/parent/_id" type="string"/>
      <bind nodeset="/covid19_rdt_provision/inputs/contact/parent/name" type="string"/>
      <bind nodeset="/covid19_rdt_provision/inputs/contact/parent/address" type="string"/>
      <bind nodeset="/covid19_rdt_provision/inputs/contact/parent/parent/_id" type="string"/>
      <bind nodeset="/covid19_rdt_provision/inputs/contact/parent/parent/name" type="string"/>
      <bind nodeset="/covid19_rdt_provision/inputs/contact/parent/parent/address" type="string"/>
      <bind nodeset="/covid19_rdt_provision/patient_uuid" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/covid19_rdt_provision/patient_name" type="string" calculate="../inputs/contact/name"/>
      <bind nodeset="/covid19_rdt_provision/patient_age_in_years" type="string" calculate="floor( difference-in-months(  /covid19_rdt_provision/inputs/contact/date_of_birth , today() ) div 12 )"/>
      <bind nodeset="/covid19_rdt_provision/patient_age_in_months" type="string" calculate="difference-in-months(  /covid19_rdt_provision/inputs/contact/date_of_birth , today() )"/>
      <bind nodeset="/covid19_rdt_provision/patient_age_in_days" type="string" calculate="floor(decimal-date-time(today()) - decimal-date-time( /covid19_rdt_provision/inputs/contact/date_of_birth ) )"/>
      <bind nodeset="/covid19_rdt_provision/age" type="string" calculate="if( /covid19_rdt_provision/patient_age_in_days  &lt; 31, 
concat( /covid19_rdt_provision/patient_age_in_days , ' days old'),
if( /covid19_rdt_provision/patient_age_in_months  &lt; 12, 
concat( /covid19_rdt_provision/patient_age_in_months , ' months old'), 
concat( /covid19_rdt_provision/patient_age_in_years , ' years old')))"/>
      <bind nodeset="/covid19_rdt_provision/preview_time_expired" type="string" calculate="format-date-time(parse-timestamp-to-date( /covid19_rdt_provision/provision/rdtoolkit-provision/android-app-outputs/rdt_session_bundle/rdt_session_time_expired ), &quot;%H:%M:%S on %e %b, %Y&quot;)"/>
      <bind nodeset="/covid19_rdt_provision/test-reference/session_id" type="string" calculate="coalesce(., uuid())"/>
      <bind nodeset="/covid19_rdt_provision/test-reference/test_id" type="string" calculate="coalesce(., uuid())"/>
      <bind nodeset="/covid19_rdt_provision/test-reference/facility_id" type="string" calculate="if(../../inputs/contact/parent/parent/_id != '', ../../inputs/contact/parent/parent/_id, ../../inputs/contact/parent/_id)"/>
      <bind nodeset="/covid19_rdt_provision/test-reference/facility_name" type="string" calculate="if(../../inputs/contact/parent/parent/_id != '', ../../inputs/contact/parent/parent/name, ../../inputs/contact/parent/name)"/>
      <bind nodeset="/covid19_rdt_provision/test-reference/facility_address" type="string" calculate="if(../../inputs/contact/parent/parent/_id != '', ../../inputs/contact/parent/parent/address, ../../inputs/contact/parent/address)"/>
      <bind nodeset="/covid19_rdt_provision/test-reference/facility_confirm_ask" type="select1"/>
      <bind nodeset="/covid19_rdt_provision/test-reference/other_facility_name" type="string" relevant="selected(../facility_confirm_ask, 'no')"/>
      <bind nodeset="/covid19_rdt_provision/test-reference/other_facility_address" type="string" relevant="selected(../facility_confirm_ask, 'no')"/>
      <bind nodeset="/covid19_rdt_provision/test-reference/other_facility_test_setting" type="select1" relevant="selected(../facility_confirm_ask, 'no')"/>
      <bind nodeset="/covid19_rdt_provision/test-reference/other_test_setting" type="string" relevant="(selected(../other_facility_test_setting , 'community_other') or selected(../other_facility_test_setting , 'healthcare_other'))"/>
      <bind nodeset="/covid19_rdt_provision/test-reference/test_reason" type="select" jr:constraintMsg="jr:itext('/covid19_rdt_provision/test-reference/test_reason:jr:constraintMsg')" constraint="not(selected(., 'symptomatic') and selected(., 'asymptomatic'))"/>
      <bind nodeset="/covid19_rdt_provision/test-reference/symptoms" type="select" relevant="selected( /covid19_rdt_provision/test-reference/test_reason , 'symptomatic')"/>
      <bind nodeset="/covid19_rdt_provision/test-reference/days_since_symptoms_began" type="int" jr:constraintMsg="jr:itext('/covid19_rdt_provision/test-reference/days_since_symptoms_began:jr:constraintMsg')" relevant="selected( /covid19_rdt_provision/test-reference/test_reason , 'symptomatic')" constraint=". &amp;gt;= 1 and . &amp;lt;= 200"/>
      <bind nodeset="/covid19_rdt_provision/spec-lot/specimen_type" type="select1"/>
      <bind nodeset="/covid19_rdt_provision/spec-lot/specimen_type_other" type="string" relevant="selected(../specimen_type, 'other')"/>
      <bind nodeset="/covid19_rdt_provision/spec-lot/rdt_lot" type="string"/>
      <bind nodeset="/covid19_rdt_provision/spec-lot/rdt_lot_expiry_date" type="date"/>
      <bind nodeset="/covid19_rdt_provision/spec-lot/rdt_lot_expiry_date_warning" readonly="true()" type="string" relevant="decimal-date-time( /covid19_rdt_provision/spec-lot/rdt_lot_expiry_date ) &lt;= decimal-date-time(today())"/>
      <bind nodeset="/covid19_rdt_provision/spec-lot/additional_notes" type="string"/>
      <bind nodeset="/covid19_rdt_provision/provision/rdtoolkit-provision/action" type="string"/>
      <bind nodeset="/covid19_rdt_provision/provision/rdtoolkit-provision/note_instructions_provision" readonly="true()" type="string"/>
      <bind nodeset="/covid19_rdt_provision/provision/rdtoolkit-provision/android-app-inputs/rdt_session_id" type="string" calculate=" /covid19_rdt_provision/test-reference/test_id "/>
      <bind nodeset="/covid19_rdt_provision/provision/rdtoolkit-provision/android-app-inputs/rdt_config_bundle/rdt_config_classifier_mode" type="string"/>
      <bind nodeset="/covid19_rdt_provision/provision/rdtoolkit-provision/android-app-inputs/rdt_config_bundle/rdt_config_session_type" type="string"/>
      <bind nodeset="/covid19_rdt_provision/provision/rdtoolkit-provision/android-app-inputs/rdt_config_bundle/rdt_config_provision_mode" type="string"/>
      <bind nodeset="/covid19_rdt_provision/provision/rdtoolkit-provision/android-app-inputs/rdt_config_bundle/rdt_config_provision_mode_data" type="string"/>
      <bind nodeset="/covid19_rdt_provision/provision/rdtoolkit-provision/android-app-inputs/rdt_config_bundle/rdt_config_flavor_one" type="string" calculate="../../../../../inputs/contact/_id"/>
