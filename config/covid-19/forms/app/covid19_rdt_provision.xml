      <bind nodeset="/data/inputs/contact/date_of_birth" type="string"/>
      <bind nodeset="/data/inputs/contact/sex" type="string"/>
      <bind nodeset="/data/inputs/contact/phone" type="string"/>
      <bind nodeset="/data/inputs/contact/phone_alternate" type="string"/>
      <bind nodeset="/data/inputs/contact/address" type="string"/>
      <bind nodeset="/data/inputs/contact/parent/_id" type="string"/>
      <bind nodeset="/data/inputs/contact/parent/name" type="string"/>
      <bind nodeset="/data/inputs/contact/parent/address" type="string"/>
      <bind nodeset="/data/inputs/contact/parent/parent/_id" type="string"/>
      <bind nodeset="/data/inputs/contact/parent/parent/name" type="string"/>
      <bind nodeset="/data/inputs/contact/parent/parent/address" type="string"/>
      <bind nodeset="/data/patient_uuid" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/data/patient_name" type="string" calculate="../inputs/contact/name"/>
      <bind nodeset="/data/patient_age_in_years" type="string" calculate="floor( difference-in-months(  /data/inputs/contact/date_of_birth , today() ) div 12 )"/>
      <bind nodeset="/data/patient_age_in_months" type="string" calculate="difference-in-months(  /data/inputs/contact/date_of_birth , today() )"/>
      <bind nodeset="/data/patient_age_in_days" type="string" calculate="floor(decimal-date-time(today()) - decimal-date-time( /data/inputs/contact/date_of_birth ) )"/>
      <bind nodeset="/data/age" type="string" calculate="if( /data/patient_age_in_days  &lt; 31, &#10;concat( /data/patient_age_in_days , ' days old'),&#10;if( /data/patient_age_in_months  &lt; 12, &#10;concat( /data/patient_age_in_months , ' months old'), &#10;concat( /data/patient_age_in_years , ' years old')))"/>
      <bind nodeset="/data/preview_time_expired" type="string" calculate="format-date-time(parse-timestamp-to-date( /data/provision/rdtoolkit-provision/android-app-outputs/rdt_session_bundle/rdt_session_time_expired ), &quot;%H:%M:%S on %e %b, %Y&quot;)"/>
      <bind nodeset="/data/test-reference/session_id" type="string" calculate="coalesce(., uuid())"/>
      <bind nodeset="/data/test-reference/test_id" type="string" calculate="coalesce(., uuid())"/>
      <bind nodeset="/data/test-reference/facility_id" type="string" calculate="if(../../inputs/contact/parent/parent/_id != '', ../../inputs/contact/parent/parent/_id, ../../inputs/contact/parent/_id)"/>
      <bind nodeset="/data/test-reference/facility_name" type="string" calculate="if(../../inputs/contact/parent/parent/_id != '', ../../inputs/contact/parent/parent/name, ../../inputs/contact/parent/name)"/>
      <bind nodeset="/data/test-reference/facility_address" type="string" calculate="if(../../inputs/contact/parent/parent/_id != '', ../../inputs/contact/parent/parent/address, ../../inputs/contact/parent/address)"/>
      <bind nodeset="/data/test-reference/facility_confirm_ask" type="string"/>
      <bind nodeset="/data/test-reference/other_facility_name" type="string" relevant="selected(../facility_confirm_ask, 'no')"/>
      <bind nodeset="/data/test-reference/other_facility_address" type="string" relevant="selected(../facility_confirm_ask, 'no')"/>
      <bind nodeset="/data/test-reference/other_facility_test_setting" type="string" relevant="selected(../facility_confirm_ask, 'no')"/>
      <bind nodeset="/data/test-reference/other_test_setting" type="string" relevant="(selected(../other_facility_test_setting , 'community_other') or selected(../other_facility_test_setting , 'healthcare_other'))"/>
      <bind nodeset="/data/test-reference/test_reason" type="string" constraint="not(selected(., 'symptomatic') and selected(., 'asymptomatic'))" jr:constraintMsg="jr:itext('/data/test-reference/test_reason:jr:constraintMsg')"/>
      <bind nodeset="/data/test-reference/symptoms" type="string" relevant="selected( /data/test-reference/test_reason , 'symptomatic')"/>
      <bind nodeset="/data/test-reference/days_since_symptoms_began" type="int" relevant="selected( /data/test-reference/test_reason , 'symptomatic')" constraint=". &amp;gt;= 1 and . &amp;lt;= 200" jr:constraintMsg="jr:itext('/data/test-reference/days_since_symptoms_began:jr:constraintMsg')"/>
      <bind nodeset="/data/spec-lot/specimen_type" type="string"/>
      <bind nodeset="/data/spec-lot/specimen_type_other" type="string" relevant="selected(../specimen_type, 'other')"/>
      <bind nodeset="/data/spec-lot/rdt_lot" type="string"/>
      <bind nodeset="/data/spec-lot/rdt_lot_expiry_date" type="date"/>
      <bind nodeset="/data/spec-lot/rdt_lot_expiry_date_warning" readonly="true()" type="string" relevant="decimal-date-time( /data/spec-lot/rdt_lot_expiry_date ) &lt;= decimal-date-time(today())"/>
      <bind nodeset="/data/spec-lot/additional_notes" type="string"/>
      <bind nodeset="/data/provision/rdtoolkit-provision/action" type="string"/>
      <bind nodeset="/data/provision/rdtoolkit-provision/note_instructions_provision" readonly="true()" type="string"/>
      <bind nodeset="/data/provision/rdtoolkit-provision/android-app-inputs/rdt_session_id" type="string" calculate=" /data/test-reference/test_id "/>
      <bind nodeset="/data/provision/rdtoolkit-provision/android-app-inputs/rdt_config_bundle/rdt_config_classifier_mode" type="string"/>
      <bind nodeset="/data/provision/rdtoolkit-provision/android-app-inputs/rdt_config_bundle/rdt_config_session_type" type="string"/>
      <bind nodeset="/data/provision/rdtoolkit-provision/android-app-inputs/rdt_config_bundle/rdt_config_provision_mode" type="string"/>
      <bind nodeset="/data/provision/rdtoolkit-provision/android-app-inputs/rdt_config_bundle/rdt_config_provision_mode_data" type="string"/>
      <bind nodeset="/data/provision/rdtoolkit-provision/android-app-inputs/rdt_config_bundle/rdt_config_flavor_one" type="string" calculate="../../../../../inputs/contact/_id"/>
      <bind nodeset="/data/provision/rdtoolkit-provision/android-app-inputs/rdt_config_bundle/rdt_config_flavor_two" type="string" calculate="../../../../../inputs/contact/name"/>
      <bind nodeset="/data/provision/rdtoolkit-provision/android-app-inputs/rdt_config_bundle/rdt_config_cloudworks_dns" type="string"/>
      <bind nodeset="/data/provision/rdtoolkit-provision/android-app-outputs/rdt_session_bundle/rdt_session_state" type="string"/>
      <bind nodeset="/data/provision/rdtoolkit-provision/android-app-outputs/rdt_session_bundle/rdt_session_time_started" type="string"/>
      <bind nodeset="/data/provision/rdtoolkit-provision/android-app-outputs/rdt_session_bundle/rdt_session_time_resolved" type="string"/>
      <bind nodeset="/data/provision/rdtoolkit-provision/android-app-outputs/rdt_session_bundle/rdt_session_time_expired" type="string"/>
      <bind nodeset="/data/provision/rdtoolkit-provision/android-app-outputs/rdt_session_bundle/rdt_session_test_profile" type="string"/>
      <bind nodeset="/data/provision/rdtoolkit-provision/preview_session_state" type="string" required="true()" readonly="true()" calculate=" /data/provision/rdtoolkit-provision/android-app-outputs/rdt_session_bundle/rdt_session_state "/>
      <bind nodeset="/data/provision/note_continue_1" readonly="true()" type="string" relevant=" /data/provision/rdtoolkit-provision/android-app-outputs/rdt_session_bundle/rdt_session_time_started  != ''"/>
      <bind nodeset="/data/provision/note_continue_2" readonly="true()" type="string" relevant=" /data/provision/rdtoolkit-provision/android-app-outputs/rdt_session_bundle/rdt_session_time_started  != ''"/>
      <bind nodeset="/data/rdt_session_other/rdt_session_other_type" type="string" required="true()" relevant="starts-with( /data/provision/rdtoolkit-provision/android-app-outputs/rdt_session_bundle/rdt_session_test_profile , &quot;generic_c19_&quot;)"/>
      <bind nodeset="/data/summary/syndrome_test_type" type="string"/>
      <bind nodeset="/data/summary/rdt_loinc_code" type="string"/>
      <bind nodeset="/data/summary/rdt_loinc_long_common_name" type="string"/>
      <bind nodeset="/data/summary/summary_header" readonly="true()" type="string"/>
      <bind nodeset="/data/summary/summary_details" readonly="true()" type="string"/>
      <bind nodeset="/data/summary/summary_name" readonly="true()" type="string"/>
      <bind nodeset="/data/summary/summary_info" readonly="true()" type="string"/>
      <bind nodeset="/data/summary/summary_test_name" readonly="true()" type="string"/>
      <bind nodeset="/data/summary/summary_test_expire" readonly="true()" type="string"/>
      <bind nodeset="/data/meta/instanceID" type="string" readonly="true()" jr:preload="uid"/>
    </model>
  </h:head>
  <h:body class="pages">
    <group appearance="field-list" ref="/data/inputs">
      <group ref="/data/inputs/user"></group>
      <group appearance="field-list" ref="/data/inputs/contact">
        <input ref="/data/inputs/contact/_id" appearance="select-contact type-person">
          <label ref="jr:itext('/data/inputs/contact/_id:label')"/>
        </input>
        <group ref="/data/inputs/contact/parent">
          <group ref="/data/inputs/contact/parent/parent"></group>
        </group>
      </group>
    </group>
    <group appearance="field-list" ref="/data/test-reference">
      <label ref="jr:itext('/data/test-reference:label')"/>
      <select1 ref="/data/test-reference/facility_confirm_ask">
        <label ref="jr:itext('/data/test-reference/facility_confirm_ask:label')"/>
        <itemset nodeset="instance('yes_no')/root/item">
          <value ref="name"/>
          <label ref="jr:itext(itextId)"/>
        </itemset>
      </select1>
      <input ref="/data/test-reference/other_facility_name">
        <label ref="jr:itext('/data/test-reference/other_facility_name:label')"/>
      </input>
      <input ref="/data/test-reference/other_facility_address">
        <label ref="jr:itext('/data/test-reference/other_facility_address:label')"/>
      </input>
      <select1 ref="/data/test-reference/other_facility_test_setting">
        <label ref="jr:itext('/data/test-reference/other_facility_test_setting:label')"/>
        <itemset nodeset="instance('test_setting')/root/item">
          <value ref="name"/>
          <label ref="jr:itext(itextId)"/>
        </itemset>
      </select1>
      <input ref="/data/test-reference/other_test_setting">
        <label ref="jr:itext('/data/test-reference/other_test_setting:label')"/>
      </input>
      <select ref="/data/test-reference/test_reason">
        <label ref="jr:itext('/data/test-reference/test_reason:label')"/>
        <itemset nodeset="instance('test_reason')/root/item">
          <value ref="name"/>
          <label ref="jr:itext(itextId)"/>
        </itemset>
      </select>
      <select ref="/data/test-reference/symptoms">
        <label ref="jr:itext('/data/test-reference/symptoms:label')"/>
        <itemset nodeset="instance('symptoms')/root/item">
          <value ref="name"/>
          <label ref="jr:itext(itextId)"/>
        </itemset>
      </select>
      <input ref="/data/test-reference/days_since_symptoms_began">
        <label ref="jr:itext('/data/test-reference/days_since_symptoms_began:label')"/>
      </input>
    </group>
    <group appearance="field-list" ref="/data/spec-lot">
      <label ref="jr:itext('/data/spec-lot:label')"/>
      <select1 ref="/data/spec-lot/specimen_type">
        <label ref="jr:itext('/data/spec-lot/specimen_type:label')"/>
        <itemset nodeset="instance('specimen_type')/root/item">
          <value ref="name"/>
          <label ref="jr:itext(itextId)"/>
        </itemset>
      </select1>
      <input ref="/data/spec-lot/specimen_type_other">
        <label ref="jr:itext('/data/spec-lot/specimen_type_other:label')"/>
      </input>
      <input ref="/data/spec-lot/rdt_lot">
        <label ref="jr:itext('/data/spec-lot/rdt_lot:label')"/>
      </input>
      <input ref="/data/spec-lot/rdt_lot_expiry_date">
        <label ref="jr:itext('/data/spec-lot/rdt_lot_expiry_date:label')"/>
      </input>
      <input ref="/data/spec-lot/rdt_lot_expiry_date_warning">
        <label ref="jr:itext('/data/spec-lot/rdt_lot_expiry_date_warning:label')"/>
      </input>
      <input ref="/data/spec-lot/additional_notes" appearance="multiline">
        <label ref="jr:itext('/data/spec-lot/additional_notes:label')"/>
      </input>
    </group>
    <group appearance="field-list" ref="/data/provision">
      <group appearance="android-app-launcher field-list" ref="/data/provision/rdtoolkit-provision">
        <input ref="/data/provision/rdtoolkit-provision/action" appearance="hidden"></input>
        <input ref="/data/provision/rdtoolkit-provision/note_instructions_provision">
          <label ref="jr:itext('/data/provision/rdtoolkit-provision/note_instructions_provision:label')"/>
        </input>
        <group appearance="android-app-inputs" ref="/data/provision/rdtoolkit-provision/android-app-inputs">
          <input ref="/data/provision/rdtoolkit-provision/android-app-inputs/rdt_session_id" appearance="hidden"></input>
          <group appearance="android-app-object" ref="/data/provision/rdtoolkit-provision/android-app-inputs/rdt_config_bundle">
            <input ref="/data/provision/rdtoolkit-provision/android-app-inputs/rdt_config_bundle/rdt_config_classifier_mode" appearance="hidden"></input>
            <input ref="/data/provision/rdtoolkit-provision/android-app-inputs/rdt_config_bundle/rdt_config_session_type" appearance="hidden"></input>
            <input ref="/data/provision/rdtoolkit-provision/android-app-inputs/rdt_config_bundle/rdt_config_provision_mode" appearance="hidden"></input>
            <input ref="/data/provision/rdtoolkit-provision/android-app-inputs/rdt_config_bundle/rdt_config_provision_mode_data" appearance="hidden"></input>
            <input ref="/data/provision/rdtoolkit-provision/android-app-inputs/rdt_config_bundle/rdt_config_flavor_one" appearance="hidden"></input>
            <input ref="/data/provision/rdtoolkit-provision/android-app-inputs/rdt_config_bundle/rdt_config_flavor_two" appearance="hidden"></input>
            <input ref="/data/provision/rdtoolkit-provision/android-app-inputs/rdt_config_bundle/rdt_config_cloudworks_dns" appearance="hidden"></input>
          </group>
        </group>
        <group appearance="android-app-outputs" ref="/data/provision/rdtoolkit-provision/android-app-outputs">
          <group appearance="android-app-object" ref="/data/provision/rdtoolkit-provision/android-app-outputs/rdt_session_bundle">
            <input ref="/data/provision/rdtoolkit-provision/android-app-outputs/rdt_session_bundle/rdt_session_state" appearance="hidden"></input>
            <input ref="/data/provision/rdtoolkit-provision/android-app-outputs/rdt_session_bundle/rdt_session_time_started" appearance="hidden"></input>
            <input ref="/data/provision/rdtoolkit-provision/android-app-outputs/rdt_session_bundle/rdt_session_time_resolved" appearance="hidden"></input>
            <input ref="/data/provision/rdtoolkit-provision/android-app-outputs/rdt_session_bundle/rdt_session_time_expired" appearance="hidden"></input>
            <input ref="/data/provision/rdtoolkit-provision/android-app-outputs/rdt_session_bundle/rdt_session_test_profile" appearance="hidden"></input>
          </group>
        </group>
        <input ref="/data/provision/rdtoolkit-provision/preview_session_state">
          <label ref="jr:itext('/data/provision/rdtoolkit-provision/preview_session_state:label')"/>
          <hint ref="jr:itext('/data/provision/rdtoolkit-provision/preview_session_state:hint')"/>
        </input>
      </group>
      <input ref="/data/provision/note_continue_1">
        <label ref="jr:itext('/data/provision/note_continue_1:label')"/>
      </input>
      <input ref="/data/provision/note_continue_2">
        <label ref="jr:itext('/data/provision/note_continue_2:label')"/>
      </input>
    </group>
    <group appearance="field-list" ref="/data/rdt_session_other">
      <input ref="/data/rdt_session_other/rdt_session_other_type">
        <label ref="jr:itext('/data/rdt_session_other/rdt_session_other_type:label')"/>
      </input>
    </group>
    <group appearance="field-list summary" ref="/data/summary">
      <input ref="/data/summary/summary_header" appearance="h2">
        <label ref="jr:itext('/data/summary/summary_header:label')"/>
      </input>
      <input ref="/data/summary/summary_details" appearance="h1 yellow">
        <label ref="jr:itext('/data/summary/summary_details:label')"/>
      </input>
      <input ref="/data/summary/summary_name" appearance="center">
        <label ref="jr:itext('/data/summary/summary_name:label')"/>
      </input>
      <input ref="/data/summary/summary_info" appearance="h1 blue">
        <label ref="jr:itext('/data/summary/summary_info:label')"/>
      </input>
      <input ref="/data/summary/summary_test_name" appearance="center">
        <label ref="jr:itext('/data/summary/summary_test_name:label')"/>
      </input>
      <input ref="/data/summary/summary_test_expire" appearance="center">
        <label ref="jr:itext('/data/summary/summary_test_expire:label')"/>
      </input>
    </group>
  </h:body>
</h:html>