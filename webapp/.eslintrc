{
  "env": {
    "browser": true,
    "jquery": true,
    "node": true
  },
  "plugins": ["compat", "angular"],
  "globals": {
    "angular": true,
    "Tour": true
  },
  "rules": {
    "compat/compat": "error",
    "no-console": "error",
    "angular/window-service": "error",
    "angular/timeout-service": "error"
  },
  "overrides": [
    {
      "files": [
        "src/js/**/*.js"
      ],
      "rules": {
        "no-console": "error"
      }
    },
    {
      "files": [
        "src/js/app.js",
        "src/js/modules/feedback.js",
        "src/js/bootstrapper/*.js"
      ],
      "rules": {
        "no-console": "off"
      }
    }
  ]
}
